<wsdl:definitions name="PS_VFQA_CRM_SOAPSubscriber" targetNamespace="http://www.vfq.techmahindra.com/SMS" xmlns:ns1="http://www.vfq.techmahindra.com/SMS" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:vfo="http://group.vodafone.com/contract/vfo/fault/v1" xmlns:vho="http://group.vodafone.com/contract/vho/header/v1" xmlns:n1="http://www.vfq.techmahindra.com/SMS" xmlns:soap11="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap="http://schemas.xmlsoap.org/soap/http" xmlns:client="http://www.vfq.techmahindra.com/SMS">
	
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.vfq.techmahindra.com/SMS" schemaLocation="../Schema/SendSMSIO.xsd"/>
			<xsd:import namespace="http://group.vodafone.com/contract/vfo/fault/v1" schemaLocation="../../VFQA_CSM_Common/Schema/Fault.xsd"/>
			<xsd:import namespace="http://group.vodafone.com/contract/vho/header/v1" schemaLocation="../../VFQA_CSM_Common/Schema/Header.xsd"/>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="PS_VFQA_CRM_SOAPSubscriberRequestMessage">
		<wsdl:part name="payload" element="ns1:SMSRequestPayload"/>
	</wsdl:message>

	<!--Supporting Messages-->
	<wsdl:message name="VodafoneFault">
		<wsdl:documentation>
		This message is used for propagating Error Context and Error Message
		</wsdl:documentation>
		<wsdl:part name="VodafoneFault" element="vfo:Fault"/>
	</wsdl:message>
	<wsdl:message name="MessageHeaders">
		<wsdl:documentation>
			This message is used as the header for SOAP request			
		</wsdl:documentation>
		<wsdl:part name="Source" element="vho:Source"/>
		<wsdl:part name="Correlation" element="vho:Correlation"/>
		<wsdl:part name="Cache" element="vho:Cache"/>
		<wsdl:part name="ServiceDocumentation" element="vho:ServiceDocumentation"/>
		<wsdl:part name="ResultStatus" element="vho:ResultStatus"/>
		<wsdl:part name="Destination" element="vho:Destination"/>
	</wsdl:message>


	<wsdl:portType name="PS_VFQA_CRM_SOAPSubscriberImpl">
		<wsdl:operation name="SMSNotification">
			<wsdl:input message="client:PS_VFQA_CRM_SOAPSubscriberRequestMessage"/>			
		</wsdl:operation>
	</wsdl:portType>
	
	
	<wsdl:binding name="PS_VFQA_CRM_SOAPSubscriberBinding" type="client:PS_VFQA_CRM_SOAPSubscriberImpl">
	<wsdl:documentation>
		This binding supports a SOAP 1.1 message over an HTTP/1.1 protocol 
	</wsdl:documentation> 
	<soap11:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/> 
	
	<wsdl:operation name="SMSNotification">
			<soap11:operation soapAction="SMSNotification" style="document"/>
			<wsdl:input name="SMSNotification">
				<!--
					Require that messages contain some custom headers for tracking, routing, auditing.
				-->
				<soap11:header message="client:MessageHeaders" part="Source" use="literal"/>
				<soap11:header message="client:MessageHeaders" part="Correlation" use="literal"/>
				<soap11:header message="client:MessageHeaders" part="Cache" use="literal"/>
				<soap11:header message="client:MessageHeaders" part="Destination" use="literal"/>
				<soap11:body use="literal"/>
			</wsdl:input>
		</wsdl:operation>
	</wsdl:binding>
		
	<wsdl:service name="PS_VFQA_CRM_SOAPSubscriber">
		<wsdl:port name="PS_VFQA_CRM_SOAPSubscriber_pt" binding="client:PS_VFQA_CRM_SOAPSubscriberBinding">
			<soap:address location="http://localhost:8001/VFQA_CRM_SOAPSubscriber/PS_VFQA_CRM_SOAPSubscriber"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>