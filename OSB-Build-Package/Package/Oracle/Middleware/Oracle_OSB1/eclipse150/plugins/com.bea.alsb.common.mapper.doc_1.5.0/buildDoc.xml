<project name="Jdt Doc Isv Build" default="all"  basedir="." >

<target name="all">
	<antcall target="createDocZip" />
</target>


<target name="createDocZip">
	<zip zipfile="${basedir}/doc.zip"
		basedir="${basedir}"
		includes="*.css,*.html,images/**,wwimages/**"
	/>
</target>

</project>














