<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/PurchaseAgree" xmlns="http://www.example.org/PurchaseAgree" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PurchaseOrder">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="orderId" type="xs:decimal"/>
				<xs:element name="products">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="product" minOccurs="1" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="partId" type="xs:decimal"/>
										<xs:element name="quantity" type="xs:decimal"/>
										<xs:element name="price" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
