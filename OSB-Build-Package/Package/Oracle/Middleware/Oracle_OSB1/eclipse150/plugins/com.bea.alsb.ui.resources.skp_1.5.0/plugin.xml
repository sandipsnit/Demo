<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
       point="org.eclipse.ui.newWizards">
		<category id="com.bea.alsb.new" name="%ALSBCategory_name">
		</category>
        <wizard
              category="com.bea.alsb.new"
              class="com.bea.alsb.ui.resources.skp.wizard.SkpWizard"
              finalPerspective="com.bea.alsb.ui.perspectives.aqualogic"
              icon="icons/new_proxy_service_provider.gif"
              id="com.bea.alsb.ui.wizards.skp"
              name="%NewSkpWizard_name"
              preferredPerspectives="com.bea.alsb.ui.perspectives.aqualogic"
              project="false">
           <description>%NewProxyServiceProviderWizard_description</description>
        </wizard>
	</extension>
	
	<extension point="org.eclipse.ui.navigator.navigatorContent">
        <commonWizard type="new" 
                      menuGroupId="com.bea.alsb.new"
                      wizardId="com.bea.alsb.ui.wizards.skp">
            <enablement>
                <or>
                    <adapt type="org.eclipse.core.resources.IResource">
				        <test forcePluginActivation="true"
				              property="org.eclipse.core.resources.projectNature"
				              value="com.bea.alsb.core.natures.alsb.project"/>
				    </adapt>
                </or>
            </enablement>
        </commonWizard>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
        <editor
              class="com.bea.alsb.ui.resources.skp.editor.SkpEditor"
              contributorClass="com.bea.alsb.ui.resources.common.AlsbEditorActionBarContributer"
              default="true"
              extensions="skp"
              icon="icons/icon_proxy_service_provider.gif"
              id="com.bea.alsb.ui.editors.skp"
              name="%SkpEditor_name">
            <contentTypeBinding contentTypeId="com.bea.alsb.core.resources.skp.content-type"/>
        </editor>
	</extension>
	<extension
	   point="org.eclipse.ui.perspectiveExtensions">
	   <perspectiveExtension targetID="com.bea.alsb.ui.perspectives.aqualogic">
	      <newWizardShortcut id="com.bea.alsb.ui.wizards.skp"/>
	   </perspectiveExtension>
	</extension>
</plugin>
