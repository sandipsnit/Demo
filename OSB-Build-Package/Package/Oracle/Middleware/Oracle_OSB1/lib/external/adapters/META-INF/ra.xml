<connector xmlns="http://java.sun.com/xml/ns/j2ee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee                                
                               http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd"
           version="1.5">

   <display-name>FTP Adapter</display-name> 
   <vendor-name>Oracle</vendor-name> 
   <eis-type>FTP</eis-type> 
   <resourceadapter-version>11</resourceadapter-version>

   <resourceadapter>
      <outbound-resourceadapter>
         <connection-definition>
		     <managedconnectionfactory-class>oracle.tip.adapter.ftp.FTPManagedConnectionFactory</managedconnectionfactory-class> 

		 <config-property>
			<config-property-name>controlDir</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>${user.dir}</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>inboundDataSource</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>none</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>outboundDataSource</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>none</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>outboundDataSourceLocal</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>none</config-property-value>
		 </config-property>

		<config-property>
			<config-property-name>outboundLockTypeForWrite</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>none</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>workingDirectory</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>default</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>defaultDateFormat</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>MMM d yyyy</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>recentDateFormat</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>MMM d HH:mm</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>ignoreFilesWithoutTimeFormat</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>serverTimeZone</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>listParserKey</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>UNIX</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>timeParserKey</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>default</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>ftpClientClass</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>default</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>host</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>localhost</config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>port</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>21</config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>username</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>password</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>accountName</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>keepConnections</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>true</config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>ftpPathSeparator</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>/</config-property-value>
		 </config-property>
		 <config-property>
			<config-property-name>ftpAbsolutePathBegin</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>/</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>changeDirectory</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>
		 <config-property>
			<config-property-name>enforceFileTypeFromSpec</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>
		 <config-property>
			<config-property-name>serverType</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>unix</config-property-value>
		 </config-property>
			  
		 <config-property>
			<config-property-name>serverLocaleLanguage</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>serverLocaleCountry</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>serverLocaleVariant</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>serverEncoding</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>useFtps</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>useImplicitSSL</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>walletLocation</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>walletPassword</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>channelMask</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>both</config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>securePort</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>990</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>keyStoreProviderName</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>oracle.security.pki.OraclePKIProvider</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>keystoreType</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>PKCS12</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>keystoreAlgorithm</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>OracleX509</config-property-value>
		 </config-property>
		 
		 <config-property>
			<config-property-name>enableCipherSuits</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>     
		 </config-property>

		 <config-property>
			<config-property-name>pkiProvider</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>OraclePKI</config-property-value>     
		 </config-property>

		 <config-property>
			<config-property-name>jsseProvider</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>OracleJSSE</config-property-value>     
		 </config-property>

		 <config-property>
			<config-property-name>trustManager</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>oracle.tip.adapter.ftp.ServerTrustManager</config-property-value>     
		 </config-property>

		 <config-property>
			<config-property-name>proxyHost</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>proxyPort</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>proxyUsername</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>proxyPassword</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>proxyType</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
		<config-property-name>proxyDefinitionFile</config-property-name>
		<config-property-type>java.lang.String</config-property-type>
		<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>useProxy</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>useSftp</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>authenticationType</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>password</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>ignorePermissionsOnFile</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>false</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>preferredKeyExchangeAlgorithm</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>diffie-hellman-group1-sha1</config-property-value>
		 </config-property>

		 <config-property>
		<config-property-name>preferredCompressionAlgorithm</config-property-name>
		<config-property-type>java.lang.String</config-property-type>
		<config-property-value>none</config-property-value>
		 </config-property>

		 <config-property>
		<config-property-name>preferredDataIntegrityAlgorithm</config-property-name>
		<config-property-type>java.lang.String</config-property-type>
		<config-property-value>hmac-md5</config-property-value>
		 </config-property>
		 <config-property>
			<config-property-name>preferredPKIAlgorithm</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>ssh-rsa</config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>privateKeyFile</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>publicKeyFile</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value></config-property-value>
		 </config-property>

		 <config-property>
			<config-property-name>preferredCipherSuite</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>aes128-cbc</config-property-value>
		 </config-property>
		
		 <config-property>
			<config-property-name>transportProvider</config-property-name>
			<config-property-type>java.lang.String</config-property-type>
			<config-property-value>socket</config-property-value>
		 </config-property>

     <connectionfactory-interface>javax.resource.cci.ConnectionFactory</connectionfactory-interface> 
     <connectionfactory-impl-class>oracle.tip.adapter.ftp.FTPConnectionFactory</connectionfactory-impl-class> 
     <connection-interface>oracle.tip.adapter.ftp.IFTPConnection</connection-interface> 
     <connection-impl-class>oracle.tip.adapter.ftp.FTPConnection</connection-impl-class> 
	 </connection-definition>

     <transaction-support>NoTransaction</transaction-support>
     
              <authentication-mechanism>
            <authentication-mechanism-type>BasicPassword</authentication-mechanism-type>
            <credential-interface>javax.resource.spi.security.PasswordCredential</credential-interface>
         </authentication-mechanism>
         <reauthentication-support>false</reauthentication-support>
      </outbound-resourceadapter>
   </resourceadapter>
</connector>
