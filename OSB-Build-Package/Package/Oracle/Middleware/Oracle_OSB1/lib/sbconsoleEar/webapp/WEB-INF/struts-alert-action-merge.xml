<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "struts-config_1_1.dtd">

<struts-config>

  <controller className="org.apache.beehive.netui.pageflow.config.PageFlowControllerConfig" inputForward="true"
    processorClass="com.bea.alsb.console.common.base.WliPageFlowRequestProcessor" nocache="true">
    <set-property value="false" property="isReturnToPageDisabled"/>
    <set-property value="false" property="isReturnToActionDisabled"/>
    <set-property  property="nocache" value="true"/>
    <set-property property="contentType" value="text/html;charset=UTF-8"/>
  </controller>

</struts-config>
