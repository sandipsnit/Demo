<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
	                           "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

    <form-beans>
        <form-bean name="SimpleForm"  type="com.bea.alsb.console.forms.SimpleForm" />
        <form-bean name="ProjectsForm" type="com.bea.alsb.console.projects.forms.ProjectsForm" />
        <form-bean name="ProjectExplorerForm" type="com.bea.alsb.console.projects.forms.ProjectExplorerForm" />
        <form-bean name="ResourceUtilityForm" type="com.bea.alsb.console.projects.forms.ResourceUtilityForm" />
        <form-bean name="LocationUtilityForm"  type="com.bea.alsb.console.projects.forms.LocationUtilityForm" />
    </form-beans>

    <global-exceptions>
        <exception key=""
                   type="java.lang.Exception"
                   handler="com.bea.alsb.console.controller.ConsoleException" />
    </global-exceptions>

    <global-forwards>
        <forward name="GlobalException"
                 contextRelative="true"
                 path="jsp/main/ErrorPage.jsp" />
    </global-forwards>

	<action-mappings>

        <action path="/ViewAllProjects"
                type="com.bea.alsb.console.projects.actions.ProjectsAction"
                name="ProjectsForm"
                attribute="ProjectsForm"
                scope="session"
                validate="true"
                parameter="ViewAll"
                input="stay">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewAllProjects.jsp"/>
        </action>

        <action path="/SortProjects"
		        type="org.apache.struts.actions.ForwardAction"
		        parameter="jsp/projects/ViewAllProjects.jsp" />

        <action path="/AddProject"
                type="com.bea.alsb.console.projects.actions.ProjectsAction"
                name="ProjectsForm"
                attribute="ProjectsForm"
                scope="session"
                validate="true"
                parameter="Add"
                input="stay">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewAllProjects.jsp"/>
        </action>

        <action path="/DeleteProject"
                type="com.bea.alsb.console.projects.actions.ProjectsAction"
                name="ProjectsForm"
                attribute="ProjectsForm"
                scope="session"
                validate="false"
                parameter="Delete">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewAllProjects.jsp"/>
        </action>

        <action path="/ManageProject"
                type="com.bea.alsb.console.projects.actions.ProjectsAction"
                name="ProjectsForm"
                attribute="ProjectsForm"
                scope="session"
                validate="false"
                parameter="Reload">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewAllProjects.jsp"/>
        </action>

        <action path="/ViewProject"
                type="com.bea.alsb.console.projects.actions.ProjectsAction"
                name="ProjectsForm"
                attribute="ProjectsForm"
                scope="session"
                validate="false"
                parameter="View">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewAllProjects.jsp"/>
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>


        <action path="/AddFolder"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="true"
                parameter="Add"
                input="stay">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/ViewFolder"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="true"
                parameter="View"
                input="stay">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/ViewTreeFolder"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="false"
                parameter="Viewfolder">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/ManageFolder"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="false"
                parameter="Reload">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/DeleteFolder"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="false"
                parameter="Delete">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/DeleteResource"
                type="com.bea.alsb.console.projects.actions.ProjectExplorerAction"
                name="ProjectExplorerForm"
                attribute="ProjectExplorerForm"
                scope="session"
                validate="false"
                parameter="DeleteResource">
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ViewProjectExplorer.jsp"/>
        </action>

        <action path="/ReturnToProjectExplorer"
                type="org.apache.struts.actions.ForwardAction"
                parameter="jsp/projects/ViewProjectExplorer.jsp" />
                
        <action path="/ViewProjectExplorer"
		        type="org.apache.struts.actions.ForwardAction"
		        parameter="jsp/projects/ViewProjectExplorer.jsp" />

        <action path="/SortProjectExplorer"
		        type="org.apache.struts.actions.ForwardAction"
		        parameter="jsp/projects/ViewProjectExplorer.jsp" />

        <action path="/ProjectReference"
                type="org.apache.struts.actions.ForwardAction"
                parameter="jsp/projects/ViewProjectReference.jsp" />

        <!-- Resource rename -->
        <action path="/ResourceRenameEntry"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="false"
                parameter="ResourceRenameEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceRename.jsp"/>
        </action>

        <action path="/RenameResource"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="true"
                parameter="RenameResource"
                input="failed">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceRename.jsp"/>
            <forward name="failed"
                     contextRelative="true"
                     path="jsp/projects/ResourceRename.jsp"/>
        </action>

        <!-- Resource move -->
        <action path="/ResourceMoveEntry"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="false"
                parameter="ResourceMoveEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceMove.jsp"/>
        </action>

        <action path="/MoveResource"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="true"
                parameter="MoveResource"
                input="failed">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceMove.jsp"/>
            <forward name="failed"
                     contextRelative="true"
                     path="jsp/projects/ResourceMove.jsp"/>
        </action>

        <!-- Resource clone -->
        <action path="/RefResourceCloneEntry"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="false"
                parameter="ResourceCloneEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceClone.jsp"/>
        </action>

        <action path="/CloneResource"
                type="com.bea.alsb.console.projects.actions.ResourceUtilityAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="true"
                parameter="CloneResource"
                input="stay">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceClone.jsp"/>
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ResourceClone.jsp"/>
        </action>

        <!-- Resource Generate -->
        <action path="/ResourceGenerateEntry"
                type="com.bea.alsb.console.projects.actions.ResourceGenerateAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="false"
                parameter="ResourceGenerateEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceGenerate.jsp"/>
        </action>

        <action path="/GenerateResource"
                type="com.bea.alsb.console.projects.actions.ResourceGenerateAction"
                name="ResourceUtilityForm"
                attribute="ResourceUtilityForm"
                scope="session"
                validate="true"
                parameter="GenerateResource"
                input="stay">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/ResourceGenerate.jsp"/>
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/ResourceGenerate.jsp"/>
        </action>

        <!-- Location rename -->
        <action path="/LocationRenameEntry"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="false"
                parameter="LocationRenameEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationRename.jsp"/>
        </action>

        <action path="/RenameLocation"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="true"
                parameter="RenameLocation"
                input="failed">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationRename.jsp"/>
            <forward name="failed"
                     contextRelative="true"
                     path="jsp/projects/LocationRename.jsp"/>
        </action>

        <!-- Location Move -->
        <action path="/LocationMoveEntry"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="false"
                parameter="LocationMoveEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationMove.jsp"/>
        </action>

        <action path="/MoveLocation"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="true"
                parameter="MoveLocation"
                input="failed">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationMove.jsp"/>
            <forward name="failed"
                     contextRelative="true"
                     path="jsp/projects/LocationMove.jsp"/>
        </action>

        <action path="/LocationCloneEntry"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="false"
                parameter="LocationCloneEntry">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationClone.jsp"/>
        </action>

        <action path="/CloneLocation"
                type="com.bea.alsb.console.projects.actions.LocationUtilityAction"
                name="LocationUtilityForm"
                attribute="LocationUtilityForm"
                scope="session"
                validate="true"
                parameter="CloneLocation"
                input="stay">
            <forward name="success"
                     contextRelative="true"
                     path="jsp/projects/LocationClone.jsp"/>
            <forward name="stay"
                     contextRelative="true"
                     path="jsp/projects/LocationClone.jsp"/>
        </action>

    </action-mappings>

    <controller processorClass="com.bea.alsb.console.common.base.SBConsoleRequestProcessor"
                inputForward="true" nocache="true"/>

</struts-config>
