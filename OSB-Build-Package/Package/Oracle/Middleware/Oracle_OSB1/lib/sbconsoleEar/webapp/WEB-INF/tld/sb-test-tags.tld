<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>sb-test</short-name>
    <uri>sb-test-tags</uri>
    <display-name>Service Bus Test Tags</display-name>
    <description>
        Tags used by the ALSB Test Framework
    </description>


    <!--
        <sb-test:page formname="" formaction="" enctype=""/>
    -->
    <tag>
        <name>page</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestPage</tag-class>
        <body-content>JSP</body-content>
        <description>
        </description>

        <attribute>
            <name>formname</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The name for the page form.
            </description>
        </attribute>
        <attribute>
            <name>formaction</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The action for the page form (HRef or String).
            </description>
        </attribute>
        <attribute>
            <name>enctype</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                if true, the form will support multipart request.
            </description>
        </attribute>

    </tag>


    <!--
        <sb-test:page-section style=""/>
    -->
    <tag>
        <name>page-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestPageSection</tag-class>
        <body-content>JSP</body-content>
        <description>
            This is the simplest and most generic section.
        </description>

        <attribute>
            <name>style</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                When specified, the section content will adopt the given style.
            </description>
        </attribute>

    </tag>


    <!--
        <sb-test:title-section icon="" title=""/>
    -->
    <tag>
        <name>title-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestTitleSection</tag-class>
        <body-content>JSP</body-content>
        <description>
        </description>

        <attribute>
            <name>icon</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The Icon for the Page Title (Icon or String)
            </description>
        </attribute>
        <attribute>
            <name>title</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The title text for the page title (BaseText or String).
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:button-section buttons=""/>
    -->
    <tag>
        <name>button-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestButtonSection</tag-class>
        <body-content>JSP</body-content>
        <description>
            Sections displaying the set of buttons for the page.
        </description>

        <attribute>
            <name>buttons</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object[]</type>
            <description>
                The list of buttons, must implement the ActionButton or
                SubmitButton struct class of this tag.
            </description>
        </attribute>

    </tag>


    <!--
        <sb-test:operation-section icon="" title=""
                                   formname="" operations=""/>
    -->
    <tag>
        <name>operation-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestOperationSection</tag-class>
        <body-content>JSP</body-content>
        <description>
            Displays a list of Links defining the available sets of executions.
        </description>

        <attribute>
            <name>icon</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The Icon for the section header (Icon or String)
            </description>
        </attribute>
        <attribute>
            <name>title</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The title text for the section header (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>desc</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The label for this data (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>formname</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The name for the page form.
            </description>
        </attribute>
        <attribute>
            <name>operations</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.taglibs.test.TestOperationSection.Operation[]</type>
            <description>
                The list of "operations".
            </description>
        </attribute>
        <attribute>
            <name>selection</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.taglibs.test.TestOperationSection.Operation</type>
            <description>
                The selected operation.
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:data-section />
    -->
    <tag>
        <name>data-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestDataSection</tag-class>
        <body-content>JSP</body-content>
        <description>
            Section used to encapsulate user data tags.
        </description>
    </tag>


    <!--
        <sb-test:expandable-data-section icon="" title=""
                                         expandable="" id="" expanded=""
                                         nostyle=""/>
    -->
    <tag>
        <name>expandable-data-section</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestExpandableDataSection</tag-class>
        <body-content>JSP</body-content>
        <description>
            Expandable/collapsable section.
        </description>
        <attribute>
            <name>icon</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The Icon for the section header (Icon or String)
            </description>
        </attribute>
        <attribute>
            <name>title</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The title text for the section header (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>desc</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The title description for the section header (BaseText or String).
            </description>
        </attribute>

        <attribute>
            <name>expandable</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Controls whether to make the section expandable/collapsable. Default to true.
            </description>
        </attribute>
        <attribute>
            <name>expanded</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Initial state. default is true.
            </description>
        </attribute>
        <attribute>
            <name>id</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The id for this section. Required if expandable is true.
            </description>
        </attribute>

        <attribute>
            <name>nostyle</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Let users control whether to put section content style.
            </description>
        </attribute>
        <attribute>
            <name>headerCellStyle</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                Let users control whether to put table cell header style.
            </description>
        </attribute>
        <attribute>
            <name>headerStyle</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                Let users control whether to put table header style.
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:input type=""
                       label="" required=""
                       name="" index="" validator="" id=""
                       value="" checked="" size="" file=""
                       />
    -->
    <tag>
        <name>input</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestInput</tag-class>
        <body-content>JSP</body-content>
        <description>
            Tag allowing user to render different kind of inputs.
            The following types are supported:
                - primitive
                - text (optionaly with a file input)
                - xml (optionaly with a file input)
                - binary (optionaly with a file input)
                - any (combination of text/xml. the choice is dynamic)
                - checkbox
                - vspacer
                - browser (one of the resource popup browser)
                - login (display a login and password)
        </description>

        <attribute>
            <name>type</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                the type of input (see tag description for the list)
            </description>
        </attribute>

        <attribute>
            <name>label</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The label for this data (BaseText or String). (required except for vspacer)
            </description>
        </attribute>
        <attribute>
            <name>labelClass</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The Style class for label. If not supplied default class will be used.
            </description>
        </attribute>
        <attribute>
            <name>desc</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The label for this data (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>required</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Whether this input is required - default to false.
            </description>
        </attribute>

        <attribute>
            <name>validator</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.common.support.ValidationResult</type>
            <description>
                Validator containing error messages. The name of the input is used
                as key of the message.
            </description>
        </attribute>

        <attribute>
            <name>name</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The HTML input name (required except for vspacer).
            </description>
        </attribute>
        <attribute>
            <name>index</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
            <description>
                For cases of arrays of input, this is the index of the input.
                Must be non-negative to be used.
            </description>
        </attribute>
        <attribute>
            <name>id</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                Used only with type='any'. The id for this input, must be unique in the page.
            </description>
        </attribute>

        <attribute>
            <name>value</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                value of this input.
            </description>
        </attribute>
        <attribute>
            <name>checked</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Used for 'checkbox' and 'any'.
            </description>
        </attribute>

        <attribute>
            <name>url</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                - browser: url of the resource popup browser.
            </description>
        </attribute>
        <attribute>
            <name>size</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
            <description>
                - text, xml, binary, any: used for the multiline input (default is 10).
                - vspacer: height of the row
            </description>
        </attribute>
        <attribute>
            <name>file</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Used for 'text', 'any' or 'xml'. Indicate whether to add a "file" input (default is true).
            </description>
        </attribute>
        <attribute>
            <name>encoding</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                Used with the attribute file to let users specify the encoding for "text" inputs.
            </description>
        </attribute>
        <attribute>
            <name>userNameDesc</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The label for this data (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>passwordDesc</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The label for this data (BaseText or String).
            </description>
        </attribute>
        <attribute>
            <name>disabled</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Used for 'checkbox' and 'any'.
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:input-headers info="" validator=""
                               name="" value="" headers=""/>
    -->
    <tag>
        <name>input-headers</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestInputHeaders</tag-class>
        <body-content>JSP</body-content>
        <description>
        </description>

        <attribute>
            <name>info</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.taglibs.test.TestInputHeaders.Info</type>
            <description>
                Set of information for the configuration of this tag.
            </description>
        </attribute>
        <attribute>
            <name>validator</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.common.support.ValidationResult</type>
            <description>
                Validator containing error messages. The name of the input is used
                as key of the message.
            </description>
        </attribute>
        <attribute>
            <name>name</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The initial header name.
            </description>
        </attribute>
        <attribute>
            <name>value</name>
            <required>no</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The initial header value.
            </description>
        </attribute>
        <attribute>
            <name>headers</name>
            <required>yes</required>
