    <!--
        <sb-test:tab-pane formname="" tabs=""/>
    -->
    <tag>
        <name>tab-pane</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestTabPane</tag-class>
        <body-content>JSP</body-content>
        <description>
        </description>

        <attribute>
            <name>formname</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The name for the page form.
            </description>
        </attribute>
        <attribute>
            <name>tabs</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.taglibs.test.TestTabPane.Tab[]</type>
            <description>
                The list of "tabs".
            </description>
        </attribute>
        <attribute>
            <name>selection</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.alsb.console.taglibs.test.TestTabPane.Tab</type>
            <description>
                The selected tab.
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:view-data xml="" binary="" text=""/>
    -->
    <tag>
        <name>view-data</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestViewData</tag-class>
        <body-content>JSP</body-content>
        <description>
            Tag able to render either text, binary or xml data.
        </description>

        <attribute>
            <name>type</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                The type of data to render. choice of xml, text or binary.
            </description>
        </attribute>
        <attribute>
            <name>data</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Object</type>
            <description>
                The data to render. String for xml and text, byte[] for binary.
            </description>
        </attribute>
        <attribute>
            <name>errorMessage</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                if error message show error icon before the message. by default its value is false
            </description>
        </attribute>
        <attribute>
            <name>keyValueSeparator</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                If true generates a separtor line after key value data, by default it is true
            </description>
        </attribute>
    </tag>


    <!--
        <sb-test:view-trace trace=""/>
    -->
    <tag>
        <name>view-trace</name>
        <tag-class>com.bea.alsb.console.taglibs.test.TestViewTrace</tag-class>
        <body-content>JSP</body-content>
        <description>
            Tag able to render the invocation trace.
        </description>

        <attribute>
            <name>trace</name>
            <required>yes</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.bea.wli.sb.test.config.TraceType</type>
            <description>
                The trace to display.
            </description>
        </attribute>
    </tag>

    <!--
           <sb-test:input-headers info="" validator=""
                                  attachments=""/>
   -->
   <tag>
       <name>input-attachments</name>
       <tag-class>com.bea.alsb.console.taglibs.test.TestAttachments</tag-class>
       <body-content>JSP</body-content>
       <description>
       </description>

       <attribute>
           <name>info</name>
           <required>yes</required>
           <rtexprvalue>true</rtexprvalue>
           <type>com.bea.alsb.console.taglibs.test.TestAttachments.Info</type>
           <description>
               Set of information for the configuration of this tag.
           </description>
       </attribute>
       <attribute>
           <name>validator</name>
           <required>no</required>
           <rtexprvalue>true</rtexprvalue>
           <type>com.bea.alsb.console.common.support.ValidationResult</type>
           <description>
               Validator containing error messages. The name of the input is used
