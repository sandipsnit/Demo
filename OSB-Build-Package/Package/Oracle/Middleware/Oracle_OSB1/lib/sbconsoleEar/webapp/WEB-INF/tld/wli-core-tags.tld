    <!--
        <core:exception throwable="" errors="" message="" onclick="" showdetails=""/>
    -->
    <tag>
        <name>exception</name>
        <tag-class>com.bea.alsb.console.common.taglibs.core.ViewException</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>throwable</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Throwable</type>
            <description>
                The exception object whose details are to be displayed.
            </description>
        </attribute>
        <attribute>
            <name>onclick</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                A JavaScript method call which gets invoked when the Done button is clicked.
            </description>
        </attribute>
        <attribute>
            <name>message</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
            <description>
                An optional message to be display instead of the default throwable.getMessage().
            </description>
        </attribute>
        <attribute>
            <name>errors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.util.List</type>
            <description>
                An optional list of additional String error messages to be displayed.
            </description>
        </attribute>
        <attribute>
            <name>showdetails</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
            <description>
                Controls whether or not a detailed link for showing stacktrace information is to be displayed.
                Default is true.
            </description>
        </attribute>
    </tag>

    <!--
    <core:event-target />
    -->
    <tag>
        <name>event-target</name>
        <tag-class>com.bea.alsb.console.common.taglibs.core.EventTarget</tag-class>
        <body-content>JSP</body-content>
        <description>
            Introduced a hidden frame of id "eventTarget".
            This tag is to be used with the "fireEvent" javascript function in utils.js
            to send an event to the server. The response is sent back to this hidden frame.
        </description>
    </tag>

    <!--
        <core:page-status messages=""/>
    -->
    <tag>
        <name>page-status</name>
        <tag-class>com.bea.alsb.console.common.taglibs.core.PageStatusTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>message</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>com.bea.alsb.console.common.support.StatusMessage</type>
        </attribute>
        <attribute>
            <name>messages</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>com.bea.alsb.console.common.support.StatusMessage[]</type>
        </attribute>
        <attribute>
            <name>diagnostics</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>com.bea.wli.config.resource.Diagnostics</type>
        </attribute>
        <attribute>
            <name>diagnosticsArray</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>com.bea.wli.config.resource.Diagnostics[]</type>
        </attribute>
        <attribute>
            <name>detailsArray</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>java.lang.Object[]</type>
        </attribute>
        <attribute>
            <name>embedInNewRow</name>
            <required>false</required>
            <rtexprvalue>yes</rtexprvalue>
            <type>boolean</type>
            <description>
                Determines whether to embed the contents within a new TR element after closing a current TR element
                Default is true.
            </description>
        </attribute>
    </tag>

