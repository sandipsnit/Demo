             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0 portal-support-1_0_0.xsd">

    <portal:directive.page contentType="text/html;charset=UTF-8"/>

    <netuix:book definitionLabel="TestDialog_Book"
                 title="Test Dialogs"
                 markupName="book" markupType="Book">
        <netuix:content>


            <!--
                        Services
            -->
            <netuix:page definitionLabel="ServiceTestDialogPage"
                         title="Service"
                         markupName="page" markupType="Page">
                <netuix:content>
                    <netuix:gridLayout description="This layout has a single placeholder."
                                       title="Single Column Layout"
                                       htmlLayoutUri="/framework/markup/layout/singlecolumn.html.txt"
                                       iconUri="/framework/markup/layout/singlecolumn.gif"
                                       markupName="singleColumnLayout" markupType="Layout"
                                       columns="1">
                        <netuix:placeholder description="The only placeholder in this layout."
                                            title="single"
                                            flow="vertical"
                                            markupName="singleColumn_columnOne" markupType="Placeholder">

                            <netuix:portletInstance title="Service"
                                                    markupType="Portlet"
                                                    contentUri="/portlets/test/service.portlet"
                                                    instanceLabel="ServiceTestDialogPortlet"/>

                        </netuix:placeholder>
                    </netuix:gridLayout>
                </netuix:content>
            </netuix:page>


