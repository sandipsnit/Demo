#if($bean.adfLibraryFilter)  
  <${bean.prefix}filter>
    <${bean.prefix}filter-name>ADFLibraryFilter</${bean.prefix}filter-name>
    <${bean.prefix}filter-class>oracle.adf.library.webapp.LibraryFilter</${bean.prefix}filter-class>
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>provider-lazy-inited</${bean.prefix}param-name>
      <${bean.prefix}param-value>true</${bean.prefix}param-value>
    </${bean.prefix}init-param>
  </${bean.prefix}filter> 
#end
#if($bean.adfLibraryFilterMapping)
  <${bean.prefix}filter-mapping>
    <${bean.prefix}filter-name>ADFLibraryFilter</${bean.prefix}filter-name>
    <${bean.prefix}url-pattern>/*</${bean.prefix}url-pattern>
    <${bean.prefix}dispatcher>FORWARD</${bean.prefix}dispatcher>
    <${bean.prefix}dispatcher>REQUEST</${bean.prefix}dispatcher>
  </${bean.prefix}filter-mapping> 
#end
#if($bean.jpsFilter)
  <${bean.prefix}filter>
    <${bean.prefix}filter-name>JpsFilter</${bean.prefix}filter-name> 
    <${bean.prefix}filter-class>oracle.security.jps.ee.http.JpsFilter</${bean.prefix}filter-class> 
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>enable.anonymous</${bean.prefix}param-name> 
      <${bean.prefix}param-value>true</${bean.prefix}param-value> 
    </${bean.prefix}init-param>
  </${bean.prefix}filter>
#end
#if($bean.jpsFilterMapping)
  <${bean.prefix}filter-mapping>
    <${bean.prefix}filter-name>JpsFilter</${bean.prefix}filter-name> 
    <${bean.prefix}url-pattern>/*</${bean.prefix}url-pattern> 
    <${bean.prefix}dispatcher>FORWARD</${bean.prefix}dispatcher> 
    <${bean.prefix}dispatcher>REQUEST</${bean.prefix}dispatcher> 
    <${bean.prefix}dispatcher>INCLUDE</${bean.prefix}dispatcher> 
  </${bean.prefix}filter-mapping>
#end
#if($bean.ojspServlet)
  <${bean.prefix}servlet>
    <${bean.prefix}servlet-name>ojsp</${bean.prefix}servlet-name>
    <${bean.prefix}servlet-class>oracle.jsp.runtimev2.JspServlet</${bean.prefix}servlet-class>
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>generic_servlet_mode</${bean.prefix}param-name>
      <${bean.prefix}param-value>single</${bean.prefix}param-value>
    </${bean.prefix}init-param>
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>page_provider</${bean.prefix}param-name>
      <${bean.prefix}param-value>oracle.adf.library.webapp.ADFJspResourceProvider</${bean.prefix}param-value>
    </${bean.prefix}init-param>
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>jsp_timeout</${bean.prefix}param-name>
      <${bean.prefix}param-value>600</${bean.prefix}param-value>
    </${bean.prefix}init-param>
    <${bean.prefix}init-param>
      <${bean.prefix}param-name>debug_mode</${bean.prefix}param-name>
      <${bean.prefix}param-value>false</${bean.prefix}param-value>
    </${bean.prefix}init-param>
    <${bean.prefix}load-on-startup>1</${bean.prefix}load-on-startup>
  </${bean.prefix}servlet>   
#end
