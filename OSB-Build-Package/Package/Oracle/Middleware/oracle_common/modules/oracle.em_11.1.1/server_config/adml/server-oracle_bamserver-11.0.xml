 <Table Name="BAM_server_enterprisemessageservice_activity_general_rate">

  <ColumnDef Name="Name" Type="String"  Key="true"/>
  <ColumnDef Name="TotalMessagesReceived.count" Type="Integer"/>
  <ColumnDef Name="TotalMessagesRejected.count" Type="Integer"/>
  <ColumnDef Name="TotalSuccessfulDeletes.rate" Type="Double"/>
  <ColumnDef Name="TotalSuccessfulInserts.rate" Type="Double"/>
  <ColumnDef Name="TotalSuccessfulUpdates.rate" Type="Double"/>
  <ColumnDef Name="TotalSuccessfulUpserts.rate" Type="Double"/>

  <ColumnDef Name="ReportingComponentNodeID" Type="String" Key="true"/>


  <Select Name="BAM_server_enterprisemessageservice_activity_general_rate_result">
   <TableFrom Name="BAM_server_enterprisemessageservice_activity_general"/>

   <Column Name="Name" Type="String">
    <ColumnFrom Name="Name"/>
   </Column>

   <Column Name="TotalMessagesReceived.count" Type="Integer">
	<ColumnFrom Name="TotalMessagesReceived.count"/>
   </Column>

   <Column Name="TotalMessagesRejected.count" Type="Integer">
    	<ColumnFrom Name="TotalMessagesRejected.count"/>
   </Column>

   <Column Name="TotalSuccessfulDeletes.rate" Type="Double">
      	<Rate Interval="300000">
