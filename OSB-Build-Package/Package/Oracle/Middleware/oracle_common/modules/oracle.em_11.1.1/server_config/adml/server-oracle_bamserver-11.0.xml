 <Table Name="BAM_server_activedatacache_dataobjects_actions">

  <ColumnDef Name="BAM_server_activedatacache_dataobjects" Type="String"  Key="true"/>
  <ColumnDef Name="DeletesPerSec.value" Type="Double"/>
  <ColumnDef Name="InsertsPerSec.value" Type="Double"/>
  <ColumnDef Name="UpdatesPerSec.value" Type="Double"/>
  <ColumnDef Name="UpsertsPerSec.value" Type="Double"/>


  <ColumnDef Name="ReportingComponentNodeID" Type="String" Key="true"/>


  <Select Name="BAM_server_activedatacache_dataobjects_actions_result">
   <TableFrom Name="BAM_server_activedatacache_dataobjects_actions"/>

   <Column Name="BAM_server_activedatacache_dataobjects" Type="String">
    <ColumnFrom Name="BAM_server_activedatacache_dataobjects"/>
   </Column>

   <Column Name="DeletesPerSec.value" Type="Double">
	<Rate Interval="300000">
		<ColumnFrom Name="Deletes.count"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="InsertsPerSec.value" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="Inserts.count"/>
		<Default>0</Default>
	</Rate>
   </Column>

   <Column Name="UpdatesPerSec.value" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="Updates.count"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="UpsertsPerSec.value" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="Upserts.count"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="ReportingComponentNodeID" Type="String"/>
  </Select>

 </Table> <!-- BAM_server_activedatacache_dataobjects_actions -->


 <!-- Rollup for dataobject actions  -->
 <Table Name="BAM_server_activedatacache_dataobjects_actions_rollup">

  <ColumnDef Name="DataObject.Deletes" Type="Integer"/>
  <ColumnDef Name="DataObject.Inserts" Type="Integer"/>
  <ColumnDef Name="DataObject.Updates" Type="Integer"/>
  <ColumnDef Name="DataObject.Upserts" Type="Integer"/>

  <ColumnDef Name="ServerName" Type="String" Key="true"/>


  <Select Name="BAM_server_activedatacache_dataobjects_actions_rollup_result">
   <TableFrom Name="BAM_server_activedatacache_dataobjects_actions"/>

   <Column Name="DataObject.Deletes" Type="Integer">
        <Aggregation Operator="Sum" Name="Deletes.count"/>
        <Default>0</Default>
   </Column>

   <Column Name="DataObject.Inserts" Type="Integer">
        <Aggregation Operator="Sum" Name="Inserts.count"/>
        <Default>0</Default>
   </Column>

   <Column Name="DataObject.Updates" Type="Integer">
        <Aggregation Operator="Sum" Name="Updates.count"/>
        <Default>0</Default>
   </Column>

   <Column Name="DataObject.Upserts" Type="Integer">
        <Aggregation Operator="Sum" Name="Upserts.count"/>
        <Default>0</Default>
   </Column>

   <GroupBy Name='ServerName' />
  </Select>

 </Table> <!-- BAM_server_activedatacache_dataobjects_actions_rollup -->

<!-- Rate Metric calculation for agreegated values of all dataobject actions  -->
<Table Name="BAM_server_activedatacache_dataobjects_actions_all">

  <ColumnDef Name="DataObject.DeletesPerSec" Type="Double"/>
  <ColumnDef Name="DataObject.InsertsPerSec" Type="Double"/>
  <ColumnDef Name="DataObject.UpdatesPerSec" Type="Double"/>
  <ColumnDef Name="DataObject.UpsertsPerSec" Type="Double"/>
  <ColumnDef Name="ServerName" Type="String" Key="true"/>


  <Select Name="BAM_server_activedatacache_dataobjects_actions_all_result">
   <TableFrom Name="oracle_bam_server:BAM_server_activedatacache_dataobjects_actions_rollup"/>

   <Column Name="DataObject.DeletesPerSec" Type="Double">
	<Rate Interval="300000">
		<ColumnFrom Name="DataObject.Deletes"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="DataObject.InsertsPerSec" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="DataObject.Inserts"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="DataObject.UpdatesPerSec" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="DataObject.Updates"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="DataObject.UpsertsPerSec" Type="Double">
	<Rate Interval="300000">
    		<ColumnFrom Name="DataObject.Upserts"/>
		<Default>0</Default>
	</Rate>
   </Column>
   <Column Name="ServerName" Type="String"/>
  </Select>

 </Table> <!-- BAM_server_activedatacache_dataobjects_actions_all -->


 <Table Name="BAM_server_activedatacache_clientreq_all">

