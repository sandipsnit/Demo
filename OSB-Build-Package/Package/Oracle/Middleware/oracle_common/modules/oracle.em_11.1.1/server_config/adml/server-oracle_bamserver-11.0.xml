        <Default>0</Default>
   </Column>

   <Column Name="ClientReq.Total" Type="Integer">
        <Aggregation Operator="Sum" Name="TotalRequests.count"/>
        <Default>0</Default>
   </Column>

   <GroupBy Name='ServerName' />
  </Select>

 </Table> <!-- BAM_server_activedatacache_clientreq_all -->

