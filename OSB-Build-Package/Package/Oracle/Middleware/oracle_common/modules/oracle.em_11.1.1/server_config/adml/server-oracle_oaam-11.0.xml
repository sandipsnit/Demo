                <Table Name="OAAM_Models_Execution">
                    <ColumnDef Type="String" Key="true" Name="ServerName"/>
                    <ColumnDef Name="ActiveThreads" Type="Integer"/>
                    <ColumnDef Name="AvgExecutionTime" Type="Double"/>
                    <ColumnDef Name="Completed" Type="Integer"/>
                    <ColumnDef Name="Completed_delta" Type="Integer"/>
                    <ColumnDef Name="MaxActive" Type="Integer"/>
                    <ColumnDef Name="MaxTime" Type="Double"/>
                    <ColumnDef Name="MinTime" Type="Double"/>
                    <ColumnDef Name="TotalTime" Type="Double"/>
                    <Select Name="OAAM_Models_Execution">
                        <TableFrom Name="OAMS.OAAM"/>
                        <Column Name="ActiveThreads" Type="Integer">
                            <ColumnFrom Name="executionTime.active"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="AvgExecutionTime" Type="Double">
                            <ColumnFrom Name="executionTime.avg"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="Completed" Type="Integer">
                            <ColumnFrom Name="executionTime.completed"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="Completed_prev" Type="Integer">
                            <Previous Index="0" Interval="300000">
                                <ColumnFrom Name="Completed"/>
                                <Default>0</Default>
                            </Previous>
                        </Column>
                        <Column Name="Completed_delta" Type="Integer">
                            <If>
                                <Condition Operator="GT">
                                    <ColumnFrom Name="Completed_prev"/>
                                    <ColumnFrom Name="Completed"/>
                                </Condition>
                                <Then>
                                    <ColumnFrom Name="Completed" />
                                </Then>
                                <Else>
                                    <Expression Operator="Minus">
                                        <ColumnFrom Name="Completed"/>
                                        <ColumnFrom Name="Completed_prev"/>
                                    </Expression>
                                </Else>
                            </If>
                            <Default>
                                0
                            </Default>
                        </Column>
                        <Column Name="MaxActive" Type="Integer">
                            <ColumnFrom Name="executionTime.maxActive"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="MaxTime" Type="Double">
                            <ColumnFrom Name="executionTime.maxTime"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="MinTime" Type="Double">
                            <ColumnFrom Name="executionTime.minTime"/>
                            <Default>0</Default>
                        </Column>
                        <Column Name="TotalTime" Type="Double">
                            <ColumnFrom Name="executionTime.time"/>
                            <Default>0</Default>
                        </Column>
                        <Where>
                            <Condition Type="String" Operator="EQ">
                                <ColumnFrom Name="Name"/>
                                <Value>Models_Execution</Value>
                            </Condition>
                        </Where>
                    </Select>
                </Table>
