      <Table Name="user_identity_provider">
            <ColumnDef Name="ServerName" Type="String" Key="true"/>
            <ColumnDef Name="Host" Type="String"/>
            <ColumnDef Name="Process" Type="String"/>
            <ColumnDef Name="TotalOperations.count" Type="Double"/>
            <ColumnDef Name="TotalFailedOperations.count" Type="Double"/>
            <ColumnDef Name="TotalOperations.time" Type="Double"/>
            <ColumnDef Name="OperationsLatency" Type="Double"/>
            <ColumnDef Name="OperationsSuccessFailure.rate" Type="Double"/>
            <Select Name="user_identity_provider_result_1">
                  <TableFrom Name="OAMS.OAM_UserIdentityProvider"/>
                  <Column Name="ServerName" Type="String">
                        <ColumnFrom Name="ServerName"/>
                  </Column>
                  <Column Name="Host" Type="String">
                        <ColumnFrom Name="Host"/>
                  </Column>
                  <Column Name="Process" Type="String">
                        <ColumnFrom Name="Process"/>
                  </Column>
                  <Column Name="TotalOperations.count" Type="Double">
                        <Expression Operator="Plus">
                              <ColumnFrom Name="assignGroupToUser.count"/>
                              <Expression Operator="Plus">
                                    <ColumnFrom Name="authenticateUser.count"/>
                                    <Expression Operator="Plus">
                                          <ColumnFrom Name="createGroup.count"/>
                                          <Expression Operator="Plus">
                                                <ColumnFrom Name="createUser.count"/>
                                                <Expression Operator="Plus">
                                                      <ColumnFrom Name="getGroups.count"/>
                                                      <Expression Operator="Plus">
                                                            <ColumnFrom Name="getUserAttributes.count"/>
                                                            <Expression Operator="Plus">
                                                                  <ColumnFrom Name="getUsers.count"/>
                                                                  <Expression Operator="Plus">
                                                                        <ColumnFrom Name="isUserAssignedToGroup.count"/>
                                                                        <Expression Operator="Plus">
                                                                              <ColumnFrom Name="removeGroup.count"/>
                                                                              <ColumnFrom Name="removeUser.count"/>
                                                                        </Expression>
                                                                  </Expression>
                                                            </Expression>
                                                      </Expression>
                                                </Expression>
                                          </Expression>
                                    </Expression>
                              </Expression>
                        </Expression>
                  </Column>
                  <Column Name="TotalFailedOperations.count" Type="Double">
                        <Expression Operator="Plus">
                              <ColumnFrom Name="assignGroupToUserFailure.count"/>
                              <Expression Operator="Plus">
                                    <ColumnFrom Name="authenticateUserFailure.count"/>
                                    <Expression Operator="Plus">
                                          <ColumnFrom Name="createGroupFailure.count"/>
                                          <Expression Operator="Plus">
                                                <ColumnFrom Name="createUserFailure.count"/>
                                                <Expression Operator="Plus">
                                                      <ColumnFrom Name="getGroupsFailure.count"/>
                                                      <Expression Operator="Plus">
                                                            <ColumnFrom Name="getUserAttributesFailure.count"/>
                                                            <Expression Operator="Plus">
                                                                  <ColumnFrom Name="getUsersFailure.count"/>
                                                                  <Expression Operator="Plus">
                                                                        <ColumnFrom Name="isUserAssignedToGroupFailure.count"/>
                                                                        <Expression Operator="Plus">
                                                                              <ColumnFrom Name="removeGroupFailure.count"/>
                                                                              <ColumnFrom Name="removeUserFailure.count"/>
                                                                        </Expression>
                                                                  </Expression>
                                                            </Expression>
                                                      </Expression>
                                                </Expression>
                                          </Expression>
                                    </Expression>
                              </Expression>
                        </Expression>
                  </Column>
                  <Column Name="TotalOperations.time" Type="Double">
                        <Expression Operator="Plus">
                              <ColumnFrom Name="AssignGroupToUser.time"/>
                              <Expression Operator="Plus">
                                    <ColumnFrom Name="AuthenticateUser.time"/>
                                    <Expression Operator="Plus">
                                          <ColumnFrom Name="CreateGroup.time"/>
                                          <Expression Operator="Plus">
                                                <ColumnFrom Name="CreateUser.time"/>
                                                <Expression Operator="Plus">
                                                      <ColumnFrom Name="GetGroups.time"/>
                                                      <Expression Operator="Plus">
                                                            <ColumnFrom Name="GetUserAttributes.time"/>
                                                            <Expression Operator="Plus">
                                                                  <ColumnFrom Name="GetUsers.time"/>
                                                                  <Expression Operator="Plus">
                                                                        <ColumnFrom Name="IsUserAssignedToGroup.time"/>
                                                                        <Expression Operator="Plus">
                                                                              <ColumnFrom Name="RemoveGroup.time"/>
                                                                              <ColumnFrom Name="RemoveUser.time"/>
                                                                        </Expression>
                                                                  </Expression>
                                                            </Expression>
                                                      </Expression>
                                                </Expression>
                                          </Expression>
                                    </Expression>
                              </Expression>
                        </Expression>
                  </Column>
            </Select>
            <Select Name="user_identity_provider_result">
                  <TableFrom Name="user_identity_provider_result_1"/>
                  <Column Name="ServerName" Type="String"/>
                  <Column Name="Host" Type="String"/>
                  <Column Name="Process" Type="String"/>
                  <Column Name="TotalOperations.count" Type="Double"/>
                  <Column Name="TotalFailedOperations.count" Type="Double"/>
                  <Column Name="TotalOperations.time" Type="Double"/>
                  <Column Name="OperationsSuccessFailure.rate" Type="Double">
                        <Expression Operator="Mult">
                              <Expression Operator="Div">
                                    <Expression Operator="Minus">
                                          <ColumnFrom Name="TotalOperations.count"/>
                                          <ColumnFrom Name="TotalFailedOperations.count"/>
                                    </Expression>
                                    <ColumnFrom Name="TotalOperations.count"/>
                                    <Default>0</Default>
                              </Expression>
                              <Value>100</Value>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Column Name="OperationsLatency" Type="Double">
                        <Expression Operator="Div">
                              <ColumnFrom Name="TotalOperations.time"/>
                              <ColumnFrom Name="TotalOperations.count"/>
                              <Default>0</Default>
                        </Expression>
                        <Default>0</Default>
                  </Column>
            </Select>
      </Table>
