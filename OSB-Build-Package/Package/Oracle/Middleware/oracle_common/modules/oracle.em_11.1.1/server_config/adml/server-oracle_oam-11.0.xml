                  </Where>
            </Select>
      </Table>
      <Table Name="token_requests">
            <ColumnDef Name="ServerName" Type="String" Key="true"/>
            <ColumnDef Name="IssueRequests.count" Type="Double"/>
            <ColumnDef Name="IssueRequestLatency" Type="Double"/>
            <ColumnDef Name="IssueRequestsFailure.count" Type="Double"/>
            <ColumnDef Name="ValidationRequests.count" Type="Double"/>
            <ColumnDef Name="ValidationRequestLatency" Type="Double"/>
            <ColumnDef Name="ValidationRequestsFailure.count" Type="Double"/>
            <ColumnDef Name="IssueRequestsSuccessFailure.rate" Type="Double"/>
            <ColumnDef Name="ValidationRequests.persec" Type="Double"/>
            <ColumnDef Name="IssueRequests.persec" Type="Double"/>
            <ColumnDef Name="ValidationRequestsSuccessFailure.rate"
                       Type="Double"/>
            <Select Name="token_requests_result">
                  <TableFrom Name="OAMS.OAM_Requests"/>
                  <Column Name="ServerName" Type="String">
                        <ColumnFrom Name="ServerName"/>
                  </Column>
                  <Column Name="IssueRequests.count" Type="Double">
                        <ColumnFrom Name="IssueRequests.completed"/>
                  </Column>
                  <Column Name="IssueRequestLatency" Type="Double">
                        <ColumnFrom Name="IssueRequests.avg"/>
                  </Column>
                  <Column Name="IssueRequestsFailure.count" Type="Double">
                        <ColumnFrom Name="IssueRequestsFailure.count"/>
                  </Column>
                  <Column Name="ValidationRequests.count" Type="Double">
                        <ColumnFrom Name="ValidateRequests.completed"/>
                  </Column>
                  <Column Name="ValidationRequestLatency" Type="Double">
                        <ColumnFrom Name="ValidateRequests.avg"/>
                  </Column>
                  <Column Name="ValidationRequestsFailure.count" Type="Double">
                        <ColumnFrom Name="ValidateRequestsFailure.count"/>
                  </Column>
                  <Column Name="IssueRequests.persec" Type="Double">
                        <Rate Interval="60000">
                              <ColumnFrom Name="IssueRequests.count"/>
                              <Default>0</Default>
                        </Rate>
                  </Column>
                  <Column Name="IssueRequestsSuccessFailure.rate" Type="Double">
                        <Expression Operator="Mult">
                              <Expression Operator="Div">
                                    <Expression Operator="Minus">
                                          <ColumnFrom Name="IssueRequests.count"/>
                                          <ColumnFrom Name="IssueRequestsFailure.count"/>
                                    </Expression>
                                    <ColumnFrom Name="IssueRequests.count"/>
                                    <Default>0</Default>
                              </Expression>
                              <Value>100</Value>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Column Name="ValidationRequests.persec" Type="Double">
                        <Rate Interval="60000">
                              <ColumnFrom Name="ValidationRequests.count"/>
                              <Default>0</Default>
                        </Rate>
                  </Column>
                  <Column Name="ValidationRequestsSuccessFailure.rate"
                          Type="Double">
                        <Expression Operator="Mult">
                              <Expression Operator="Div">
                                    <Expression Operator="Minus">
                                          <ColumnFrom Name="ValidationRequests.count"/>
                                          <ColumnFrom Name="ValidationRequestsFailure.count"/>
                                    </Expression>
                                    <ColumnFrom Name="ValidationRequests.count"/>
                                    <Default>0</Default>
                              </Expression>
                              <Value>100</Value>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Where>
                        <And>
                              <Or>
                                    <Condition Type="String" Operator="EQ">
                                          <ColumnFrom Name="ServerName"/>
                                          <Value Name="ServerNames.parameter"/>
                                    </Condition>
