<Adml xmlns="http://www.oracle.com/iAS/aggregator"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      TableSpace="oracle_oam">
      <Table Name="oam_controller">
            <ColumnDef Name="Host" Type="String"/>
            <ColumnDef Name="ServerName" Type="String" Key="true"/>
            <ColumnDef Name="AuthnRequests.persec" Type="Double"/>
            <ColumnDef Name="AuthnLatency" Type="Double"/>
            <ColumnDef Name="AuthnSuccessFailure.rate" Type="Double"/>
            <ColumnDef Name="AuthzRequests.persec" Type="Double"/>
            <ColumnDef Name="AuthzLatency" Type="Double"/>
            <ColumnDef Name="AuthzSuccessFailure.rate" Type="Double"/>
            <Select Name="oam_controller_result">
                  <TableFrom Name="OAMS.OAM_OAMController"/>
                  <Column Name="ServerName" Type="String">
                        <ColumnFrom Name="ServerName"/>
                  </Column>
                  <Column Name="Host" Type="String">
                        <ColumnFrom Name="Host"/>
                  </Column>
                  <Column Name="AuthnRequests.persec" Type="Double">
                        <Rate Interval="60000">
                              <ColumnFrom Name="AuthnRequests.count"/>
                              <Default>0</Default>
                        </Rate>
                  </Column>
                  <Column Name="AuthnSuccessFailure.rate" Type="Double">
                        <Expression Operator="Mult">
                              <Expression Operator="Div">
                                    <ColumnFrom Name="ValidateCredentialsSuccess.count"/>
                                    <ColumnFrom Name="ValidateCredentials.completed"/>
                                    <Default>0</Default>
                              </Expression>
                              <Value>100</Value>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Column Name="AuthnLatency" Type="Double">
                        <Expression Operator="Plus">
                              <ColumnFrom Name="AuthnProcess.avg"/>
                              <ColumnFrom Name="PreAuthnProcess.avg"/>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Column Name="AuthzRequests.persec" Type="Double">
                        <Rate Interval="60000">
                              <ColumnFrom Name="AuthzRequests.count"/>
                              <Default>0</Default>
                        </Rate>
                  </Column>
                  <Column Name="AuthzLatency" Type="Double">
                        <ColumnFrom Name="Authz.avg"/>
                        <Default>0</Default>
                  </Column>
                  <Column Name="AuthzSuccessFailure.rate" Type="Double">
                        <Expression Operator="Mult">
                              <Expression Operator="Div">
                                    <ColumnFrom Name="AuthzSuccess.count"/>
                                    <ColumnFrom Name="AuthzRequests.count"/>
                                    <Default>0</Default>
                              </Expression>
                              <Value>100</Value>
                        </Expression>
                        <Default>0</Default>
                  </Column>
                  <Where>
                        <Or>
                              <Condition Type="String" Operator="EQ">
                                    <ColumnFrom Name="ServerName"/>
                                    <Value Name="ServerNames.parameter"/>
                              </Condition>
                              <Condition Type="String" Operator="EQ">
                                    <Value Name="ServerNames.parameter"
                                           xsi:nil="true"/>
                                    <Null/>
                              </Condition>
                        </Or>
                  </Where>
            </Select>
      </Table>
