 <Table Name="ampool">
  <ColumnDef Name="ServerName" Type="String" Key="true"/>
  <ColumnDef Name="module" Type="String" Key="true"></ColumnDef>
  <ColumnDef Name="appName" Type="String" Key="true"></ColumnDef>
  <ColumnDef Name="avgCreationTime" Type="Double"></ColumnDef>
  <ColumnDef Name="availableInstances" Type="Integer"></ColumnDef>
  <ColumnDef Name="instanceCheckedIn.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="instanceCheckedOut.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="instanceCheckedOutFailed.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="instanceCount" Type="Integer"></ColumnDef>
  <ColumnDef Name="instanceCreated.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="instanceRemoved.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="instanceReused.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="refInstanceRecycled.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="stateActivated.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="statePassivated.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="unrefInstanceRecycled.throughput" Type="Double"></ColumnDef>
  <ColumnDef Name="request.throughput" Type="Double"></ColumnDef>
  <Select Name="ampool_main_result">
   <TableFrom Name="weblogic_j2eeserver:ampool_no_rate"/>
   <Column Name="ServerName" Type="String"/>
   <Column Name="module" Type="String">
    <ColumnFrom Name="module"/>
   </Column>
   <Column Name="appName" Type="String">
    <ColumnFrom Name="appName"/>
   </Column>
   <Column Name="avgCreationTime" Type="Double">
    <Rate Interval="300000">
     <ColumnFrom Name="appModuleCreationTime.time"/>
     <ColumnFrom Name="appModuleCreationTime.completed"/>
     <Default>0</Default>
    </Rate>
   </Column>
   <Column Name="availableInstances" Type="Integer">
    <ColumnFrom Name="availableInstanceCount.value"/>
   </Column>
   <Column Name="instanceCheckedIn.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceCheckedIn.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="instanceCheckedOut.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceCheckedOut.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="instanceCheckedOutFailed.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceCheckedOutFailed.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="instanceCount" Type="Integer">
    <ColumnFrom Name="instanceCount.value"/>
   </Column>
   <Column Name="instanceCreated.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceCreated.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="instanceRemoved.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceRemoved.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="instanceReused.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="instanceReused.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="refInstanceRecycled.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="refInstanceRecycled.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="stateActivated.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="stateActivated.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="statePassivated.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="statePassivated.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="unrefInstanceRecycled.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="unrefInstanceRecycled.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Column Name="request.throughput" Type="Double">
    <Expression Operator="Mult">
     <Rate Interval="300000">
      <ColumnFrom Name="availableInstanceCount.count"/>
      <Default>0</Default>
     </Rate>
     <Value>60.0</Value>
    </Expression>
   </Column>
   <Where>
    <And>
      <And>
        <Condition Operator="NE" Type="String">
          <ColumnFrom Name="module"/>
          <Null/>
        </Condition>
        <Condition Operator="NE" Type="String">
           <ColumnFrom Name="appName"/>
           <Null/>
        </Condition>
     </And>
     <Or>
      <Condition Type="String" Operator="EQ">
       <ColumnFrom Name="module"/>
       <Value Name="module.parameter"/>
      </Condition>
      <Condition Type="String" Operator="EQ">
       <Value Name="module.parameter" xsi:nil="true"/>
       <Null/>
      </Condition>
     </Or>
     <Or>
      <Condition Type="String" Operator="EQ">
       <ColumnFrom Name="appName"/>
       <Value Name="appName.parameter"/>
      </Condition>
      <Condition Type="String" Operator="EQ">
       <Value Name="appName.parameter" xsi:nil="true"/>
       <Null/>
      </Condition>
     </Or>
     <Or>
      <Condition Type="String" Operator="EQ">
       <ColumnFrom Name="serverName"/>
       <Value Name="serverName.parameter"/>
      </Condition>
      <Condition Type="String" Operator="EQ">
       <Value Name="serverName.parameter" xsi:nil="true"/>
       <Null/>
      </Condition>
     </Or>
    </And>
   </Where>
  </Select>
 </Table>
