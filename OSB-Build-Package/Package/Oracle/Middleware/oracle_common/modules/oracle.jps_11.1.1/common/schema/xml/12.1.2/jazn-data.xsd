    <xsd:complexType name="JAZN-DATAsubjectType">
        <xsd:annotation>
            <xsd:documentation>
                This element specfies a grantee.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:choice>
                <xsd:element name="principals" type="JAZN-DATAprincipalsType" minOccurs="0">
