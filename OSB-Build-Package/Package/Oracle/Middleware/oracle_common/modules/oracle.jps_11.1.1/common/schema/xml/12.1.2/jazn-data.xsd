        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="display-name" type="xsd:string"/>
            <xsd:element name="description" type="xsd:string"/>
            <xsd:element name="target" type="JAZN-DATArolepolicytargetType"/>
            <xsd:element name="rule-refs" type="JAZN-DATArule-refsType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATArolepolicytargetType">
        <xsd:sequence>
            <xsd:element name="target-resources" type="JAZN-DATAtargetresourcesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="target-resource-expressions" type="JAZN-DATAtargetresourceexpressionsType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAtargetresourcesType">
        <xsd:sequence>
            <xsd:element name="target-resource" type="JAZN-DATAtargetresourceType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAtargetresourceType">
        <xsd:all>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="type-name-ref" type="xsd:string"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAtargetresourceexpressionsType">
        <xsd:sequence>
            <xsd:element name="target-resource-expression" type="JAZN-DATAtargetresourceexpressionType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAtargetresourceexpressionType">
        <xsd:all>
            <xsd:element name="resource-expression" type="xsd:string"/>
            <xsd:element name="type-name-ref" type="xsd:string"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAobligationType">
        <xsd:sequence>
            <xsd:element name="obligationExpression" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="obligationId" type="xsd:string" use="required"/>
        <xsd:attribute name="effectType" type="xsd:string" use="required"/>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAadviceType">
        <xsd:sequence>
            <xsd:element name="adviceExpression" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="adviceId" type="xsd:string" use="required"/>
        <xsd:attribute name="appliesTo" type="xsd:string" use="required"/>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAcombinerParameterType">
        <xsd:sequence>
            <xsd:element name="attributeValue" type="JAZN-DATAattributeValueType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="parameterName" type="xsd:string" use="required"/>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAruleCombinerParameterType">
        <xsd:complexContent>
            <xsd:extension base="JAZN-DATAcombinerParameterType">
                <xsd:attribute name="ruleId" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyCombinerParameterType">
        <xsd:complexContent>
            <xsd:extension base="JAZN-DATAcombinerParameterType">
                <xsd:attribute name="policyId" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicySetCombinerParameterType">
        <xsd:complexContent>
            <xsd:extension base="JAZN-DATAcombinerParameterType">
                <xsd:attribute name="policySetId" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyIssuerType">
        <xsd:sequence>
            <xsd:element name="attribute" type="JAZN-DATAattributesType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="content" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAconstraintType">
        <xsd:annotation>
            <xsd:documentation>
                All AVAs are ANDed in a constraint.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attributeValueAssertion" type="JAZN-DATAattributeValueAssertionType" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattributeValueAssertionType">
        <xsd:sequence>
            <xsd:element name="attribute" type="JAZN-DATAattributesType" minOccurs="1"/>
            <xsd:element name="matchingFunction" type="xsd:string" minOccurs="1"/>
            <xsd:element name="mustBePresent" type="xsd:boolean" minOccurs="1"/>
            <xsd:element name="attributeValue" type="JAZN-DATAattributeValueType" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattributeValueType">
        <xsd:sequence>
            <xsd:element name="value" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="dataType" type="xsd:string" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAactioncollectionsType">
        <xsd:annotation>
            <xsd:documentation>This element specifies a set of action collections.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="action-collection" type="JAZN-DATAactioncollectionType" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAactioncollectionType">
        <xsd:annotation>
            <xsd:documentation>This type specifies an action collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the name of the action collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the display name of the action collection. May be used by GUI tools.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This elements specifies the description that describes the action collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="actions" type="JAZN-DATAactions-listType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element lists the actions in the action collection</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="action-collection-refs" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element lists the action collection references in the  action collection.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ref" type="xsd:string" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="implied-actions" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element lists the implied actions map in the action collection.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="implied-action" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>This element representes an implied action mapping.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
                                        <xsd:annotation>
                                            <xsd:documentation>This element specifies the name of the implied action.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="actions" type="JAZN-DATAactions-listType" minOccurs="0" maxOccurs="1">
                                        <xsd:annotation>
                                            <xsd:documentation>This element lists the actions in the implied action.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAactions-listType">
        <xsd:annotation>
            <xsd:documentation>This type represents an actions list</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="action" type="xsd:string" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattributecollectionsType">
        <xsd:annotation>
            <xsd:documentation>This type specifies a set of attribute collections.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute-collection" type="JAZN-DATAattributecollectionType" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattributecollectionType">
        <xsd:annotation>
            <xsd:documentation>This type specifies an attribute collection.</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the name of the attribute collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the display name of the attribute collection. May be used by GUI tools.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This elements specifies the description that describes the attribute collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="policy-set-ref" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element defines the Policy Set protecting the operations performed on this AttributeCollection by any application user.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="handler-schema-name" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the schema of the attribute collection's handler function.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="handler-package-name" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the package of the attribute collection's handler function.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="handler-function-name" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the name of the attribute collection's handler function.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attribute-refs" type="JAZN-DATAattribute-refs" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element specifies the list of attributes referred in this collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattribute-refs">
        <xsd:annotation>
            <xsd:documentation>This type represents a list of attribute references</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute-ref" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>This element represents an attribute reference</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>This element specifies the name of the attribute referred.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:choice minOccurs="0" maxOccurs="1">
                            <xsd:element name="default-value" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation>This element specifies default value for a single-valued attribute.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="default-values">
                                <xsd:annotation>
                                    <xsd:documentation>This element specifies default value for a multiple-valued attribute.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="default-value" type="xsd:string" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="event-type" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>This element specifies the event associated with the attribute.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAdatasec-column-attrsType">
        <xsd:sequence>
            <xsd:element name="datasec-column-attr" type="JAZN-DATAdatasec-column-attrType" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAdatasec-column-attrType">
        <xsd:sequence>
            <xsd:element name="datasec-column-attr-name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="datasec-column-attr-display-name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="datasec-column-attr-type" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyset-aclparametersType">
        <xsd:sequence>
            <xsd:element name="policyset-acl-parameter" type="JAZN-DATApolicyset-aclparameterType" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyset-aclparameterType">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="value" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="type" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyset-databaseResourcesType">
        <xsd:sequence>
            <xsd:element name="policyset-database-resource" type="JAZN-DATApolicyset-databaseResourceType" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicyset-databaseResourceType">
        <xsd:sequence>
            <xsd:element name="database-resource" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
