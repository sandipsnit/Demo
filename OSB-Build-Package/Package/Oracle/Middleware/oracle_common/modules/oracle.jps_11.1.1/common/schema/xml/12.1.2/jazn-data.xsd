        <xsd:annotation>
            <xsd:documentation>
                This element specifies a principal.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the name of the principal.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="class" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
