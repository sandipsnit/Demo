            <xsd:element name="permission-set-refs" type="JAZN-DATApolicy-permission-set-refsType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="extended-attributes" type="JAZN-DATAextended-attributesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="envConstraint" type="JAZN-DATAconstraintType" minOccurs="0"/>
        </xsd:sequence>
