<?xml version="1.0" encoding="UTF-8"?>
<!-- changeLog
    csapra: Enhanced schema to support more schema enhancements for triton.
    csapra: Enhanced schema to support resource, resource type, policy, ps, rule changes for triton.
    sudasrid: Enhanced schema to support action and attribute collections
    jensen: Enhanced schema to support sharable attribute for rule
    acao: Enhanced schema to support PS2 enhancements for XACML3 Target.
    vrjoshi: Enhanced schema to support PS2 enhancements for XACML.
    dhgoswam: enhanced schema to support advanced policies support for 11.1.1.4.0
    dhgoswam: enhanced schema to support resource catalog support for 11.1.1.3.0
    author: rkng
    component: JPS
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xsd:element name="jazn-data" type="JAZN-DATAjazn-dataType">
        <xsd:annotation>
            <xsd:documentation>This element is the top level element for Oracle's JPS DataStore.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="JAZN-DATAjazn-dataType">
        <xsd:sequence>
            <xsd:element name="jazn-realm" type="JAZN-DATAjazn-realmType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is the set of realms in the identity store.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="policy-store" type="JAZN-DATApolicyStoreType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is the policy information</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="system-policy" type="JAZN-DATAsystem-policyType">
                <xsd:annotation>
                    <xsd:documentation>TBD</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="schema-major-version" type="xsd:integer" fixed="11">
            <xsd:annotation>
                <xsd:documentation>The element specifies the major version of this schema.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="schema-minor-version" type="xsd:string" fixed="1.1.6.0">
            <xsd:annotation>
                <xsd:documentation>This element specifies the minor version of this schema.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <!--  identity store schema  -->
    <xsd:complexType name="JAZN-DATAjazn-realmType">
        <xsd:annotation>
            <xsd:documentation>
                realm type
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="realm" type="JAZN-DATArealmType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="default" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>
                    This attribute specifies the default realm among the set of realms.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAvariable-definitionsType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="variable-definition" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="expression" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="enabled" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="encoded-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="givenName" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAattributesType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:element name="attribute-namespace" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="attribute-data-type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="attribute-category" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="attribute-type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="attribute-issuer" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="is-single-valued" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="presence-required" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="extended-attributes" type="JAZN-DATAextended-attributesType" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATArule-combining-algsType">
        <xsd:annotation>
            <xsd:documentation>This defines the rule combining algorithms</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="rule-combining-alg" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="execution-frequency" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="principal-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="resource-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="action-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="env-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="match-info" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="java-class-name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="enabled" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="encoded-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="givenName" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicy-combining-algsType">
        <xsd:annotation>
            <xsd:documentation>This defines the policy combining algorithms</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="combining-alg" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="execution-frequency" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="principal-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="resource-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="action-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="env-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="match-info" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="java-class-name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="enabled" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="encoded-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="givenName" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAfunctionsType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="function" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:element name="function-paramters" type="JAZN-DATAfunction-parameterType" minOccurs="0"/>
                        <xsd:element name="function-return-type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="function-class" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="function-category" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="extended-attributes" type="JAZN-DATAextended-attributesType" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAfunction-parameterType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="function-paramter" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAconfigurationsType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="configuration" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATArealmType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a realm.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the name of the realm.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="users" type="JAZN-DATAusersType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the set of users in this realm.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="roles" type="JAZN-DATArolesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the set of roles in this realm.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAusersType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies the set of users.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="user" type="JAZN-DATAuserType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies a user in this realm.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAuserType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a user.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the name of the user.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="display-name" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the display name of the user. May be used by GUI tools.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element contains a description that describes the
                        user.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="guid" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element contains a unique identifier used to
                        reference the user.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="credentials" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This Element contains the authentication password for
                        the user. It can be configured to be in cleartext or
                        obfuscated form.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
        <xsd:attribute name="deactivated" type="trueFalseType" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                This attribute specifies if the user is activated or not.
             </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATArolesType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a set of enterprise (idstore) roles/groups.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="role" type="JAZN-DATAroleType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies an idstore role.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAroleType">
        <xsd:all>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the role name.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="display-name" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the display name of the role.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element contains a description that describes the
                        role.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="guid" type="xsd:string" minOccurs="0"/>
            <xsd:element name="members" type="JAZN-DATAmembersType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element contains a list of users or roles that are
                        members of this role.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="owners" type="JAZN-DATAownersType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies owners of this role.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAmembersType">
        <xsd:sequence>
            <xsd:element name="member" type="JAZN-DATAmemberType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies a role member.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAmemberType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a role member.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="type" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the type of the role member.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the name of the role member.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAownersType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a role owner.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="owner" type="JAZN-DATAownerType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAownerType">
        <xsd:all>
            <xsd:element name="type" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the type of the role member.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the name of the member.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <!-- Policy Store schema -->
    <xsd:complexType name="JAZN-DATApolicyStoreType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies the authorization policy.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="jazn-permission-classes" type="JAZN-DATAjazn-permission-classesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        RESERVED FOR FUTURE USE.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jazn-principal-classes" type="JAZN-DATAjazn-principal-classesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        RESERVED FOR FUTURE USE.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="globalpolicy" type="JAZN-DATAapplicationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the global policy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="applications" type="JAZN-DATAapplicationsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies a set of application policies.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jazn-policy" type="JAZN-DATAjazn-policyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the global or system policy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAapplicationsType">
        <xsd:sequence>
            <xsd:element name="application" type="JAZN-DATAapplicationType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies an application policy.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAapplicationOneUserPropertyType">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="value" type="xsd:string" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAapplicationUserPropertiesType">
        <xsd:sequence>
            <xsd:element name="property" type="JAZN-DATAapplicationOneUserPropertyType" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAapplicationType">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the policy context identifier.
                        Typically, this is the name of the application during
                        deployment.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="user-properties" type="JAZN-DATAapplicationUserPropertiesType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This element is the user properties of the application policy.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This element is a description of the application policy.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="variable-definitions" type="JAZN-DATAvariable-definitionsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for varialbe definitions. Currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="rule-combining-algs" type="JAZN-DATArule-combining-algsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for rulecombining algorithms.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="combining-algs" type="JAZN-DATApolicy-combining-algsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for policy combining algorithms.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="configurations" type="JAZN-DATAconfigurationsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for configuration definitions. Currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attributes" type="JAZN-DATAattributesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is the attribute definition of the advanced policy</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="functions" type="JAZN-DATAfunctionsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is the function definition of the advanced policy</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="rules" type="JAZN-DATArulesType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="obligations" type="JAZN-DATAobligationType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>This is for policy obligation definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="advices" type="JAZN-DATAadviceType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>This is for policy advice definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="app-roles" type="JAZN-DATAapprolesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                                    This element specifies the set of application roles in this application.
                            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="role-categories" type="JAZN-DATArole-categoriesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for role category definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="resource-types" type="JAZN-DATAresource-typesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for resource type definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="resources" type="JAZN-DATAresourcesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for resource instance definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="permission-sets" type="JAZN-DATApermission-setsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is for permission set definitions</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="admin-policy" type="JAZN-DATAadmin-policyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>This is the admin policy</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="policies" type="JAZN-DATApoliciesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the runtime advanced policy of this application. The set of obligations and rules.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="policySets" type="JAZN-DATApolicySetType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                                        This element specifies the runtime advanced policy set of this application.
                                </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="action-collections" type="JAZN-DATAactioncollectionsType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This elements specifies the action collections in this application.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attribute-collections" type="JAZN-DATAattributecollectionsType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>This elements specifies the attribute collections in this application.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="policy-domains" type="JAZN-DATApolicy-domainsType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the policy domains for this application.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jazn-policy" type="JAZN-DATAjazn-policyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
