    <xsd:complexType name="JAZN-DATApolicySetType">
        <xsd:annotation>
            <xsd:documentation>
                            This element specifies a policy set.
                        </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="policySet" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0"/>
                        <xsd:choice>
                            <!--CHOICE INTRODUCED BETWEEN EXISTING TARGET DEFINITION AND THE NEW MULTI_TARGET DEFINITION -->
                            <xsd:element name="target" type="JAZN-DATAtargetType" maxOccurs="unbounded"/>
                            <xsd:element name="multi-target" type="JAZN-DATAMultiTarget" maxOccurs="1"/>
                        </xsd:choice>
                        <xsd:element name="issuer" type="JAZN-DATApolicyIssuerType" minOccurs="0"/>
                        <xsd:element name="version" type="xsd:string" minOccurs="1"/>
                        <xsd:element name="givenName" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="policSetDefaults" type="xsd:string" minOccurs="0"/>
                        <xsd:choice maxOccurs="unbounded">
                            <xsd:element name="combinerParameter" type="JAZN-DATAcombinerParameterType" minOccurs="0" maxOccurs="unbounded"/>
                            <xsd:element name="policyCombinerParameter" type="JAZN-DATApolicyCombinerParameterType" minOccurs="0" maxOccurs="unbounded"/>
                            <xsd:element name="policySetCombinerParameter" type="JAZN-DATApolicySetCombinerParameterType" minOccurs="0" maxOccurs="unbounded"/>
                            <xsd:element name="policy-refs" type="JAZN-DATApolicy-refsType" minOccurs="0"/>
                            <xsd:element name="policySet-refs" type="JAZN-DATApolicySet-refsType" minOccurs="0"/>
                        </xsd:choice>
                        <xsd:element name="envConstraint" type="JAZN-DATAconstraintType" minOccurs="0"/>
                        <xsd:element name="allowObligation-refs" type="JAZN-DATAobligation-refsType" minOccurs="0"/>
                        <xsd:element name="denyObligation-refs" type="JAZN-DATAobligation-refsType" minOccurs="0"/>
                        <xsd:element name="allowAdvice-refs" type="JAZN-DATAadvice-refsType" minOccurs="0"/>
                        <xsd:element name="denyAdvice-refs" type="JAZN-DATAadvice-refsType" minOccurs="0"/>
                        <xsd:element name="maxDelegationDepth" type="xsd:integer" minOccurs="0"/>
                        <xsd:element name="encoded-attributes" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="extended-attributes" type="JAZN-DATAextended-attributesType" minOccurs="0"/>
                        <xsd:element name="policyset-type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="policyset-category" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="action-collection-refs" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="policyset-acl-parameters" type="JAZN-DATApolicyset-aclparametersType" minOccurs="0"/>
                        <xsd:element name="policyset-database-resources" type="JAZN-DATApolicyset-databaseResourcesType" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAjazn-policyType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a policy.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="grant" type="JAZN-DATAgrantType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies a set of grants.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAgrantType">
        <xsd:annotation>
            <xsd:documentation>
                This element specifies a grant of a permission to a grantee.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="description" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element contains a description about the grant.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="display-name" type="JAZN-DATAstringType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="enabled" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="grantee" type="JAZN-DATAgranteeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the grantee who is granted the
                        permission(s).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="permissions" type="JAZN-DATApermissionsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies a set of permissions.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="permission-set-refs" type="JAZN-DATApermission-set-refsType" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApermission-set-refsType">
