        </xsd:annotation>
        <xsd:all>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        This element specifies the reference of the permission set.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATApolicy-permission-set-refsType">
        <xsd:sequence>
            <xsd:element name="permission-set-ref" type="xsd:string" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="JAZN-DATAtargetType">
        <xsd:annotation>
            <xsd:documentation>This is the advanced policy target.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="subject-combining-alg" type="xsd:string" minOccurs="0" maxOccurs="1">
