    <escapeSlashes>true</escapeSlashes>
    <kerberos>false</kerberos>
    <useDNS>No</useDNS>
    <ssl>
        <protocols/>
        <cipherSuites>
            <cipher>SSL_RSA_WITH_RC4_128_MD5</cipher>
            <cipher>SSL_RSA_WITH_RC4_128_SHA</cipher>
            <cipher>SSL_RSA_WITH_3DES_EDE_CBC_SHA</cipher>
            <cipher>SSL_DHE_RSA_WITH_3DES_EDE_CBC_SHA</cipher>
            <cipher>SSL_DHE_DSS_WITH_3DES_EDE_CBC_SHA</cipher>
            <cipher>SSL_RSA_WITH_DES_CBC_SHA</cipher>
            <cipher>SSL_DHE_RSA_WITH_DES_CBC_SHA</cipher>
            <cipher>SSL_DHE_DSS_WITH_DES_CBC_SHA</cipher>
            <cipher>SSL_RSA_EXPORT_WITH_RC4_40_MD5</cipher>
            <cipher>SSL_RSA_EXPORT_WITH_DES40_CBC_SHA</cipher>
            <cipher>SSL_DHE_RSA_EXPORT_WITH_DES40_CBC_SHA</cipher>
            <cipher>SSL_DHE_DSS_EXPORT_WITH_DES40_CBC_SHA</cipher>
            <cipher>SSL_DH_anon_WITH_RC4_128_MD5</cipher>
            <cipher>SSL_DH_anon_WITH_3DES_EDE_CBC_SHA</cipher>
            <cipher>SSL_DH_anon_WITH_DES_CBC_SHA</cipher>
            <cipher>SSL_DH_anon_EXPORT_WITH_RC4_40_MD5</cipher>
            <cipher>SSL_DH_anon_EXPORT_WITH_DES40_CBC_SHA</cipher>
        </cipherSuites>
    </ssl>
