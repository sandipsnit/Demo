<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
	<!--
		====================================================================
	-->
	<!--
		This configuration file contains set of bootstrap properties. An OWSM
		agent instance will use these bootstrap connection information
		specified during installation to connect to the OWSM Policy Manager.

		Add attribute orares:bootstrap.pm.url={protocol}://{hostname}:{port}
		and orares:bootstrap.pm.csf-key="{papCsfKey}" to connect to the policy
		manager. Please refer resources.xsd for each bootstrap property
		description.
	-->

	<!--
		====================================================================
	-->
<orares:configuration
	orares:bootstrap.keystore.ssl.alias="mykey"
	orares:bootstrap.keystore.csf-key="keystore-csf-key"
	orares:bootstrap.keystore.path="./default-keystore.jks"
	orares:bootstrap.truststore.csf-key="keystore-csf-key"
	orares:bootstrap.truststore.path="./default-keystore.jks"
	orares:bootstrap.ssl.twoway="false" orares:bootstrap.keystore.type="JKS"
	xmlns:sca11="http://docs.oasis-open.org/ns/opencsa/sca/200903"
	xmlns:orawsp="http://schemas.oracle.com/ws/2006/01/policy"
	xmlns:orares="http://xmlns.oracle.com/wsm/resources" xmlns:wsp15="http://www.w3.org/ns/ws-policy"
	xmlns:ns7="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
	xmlns:ns8="http://www.w3.org/2000/09/xmldsig#"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:orarepo="http://xmlns.oracle.com/wsm/policy/repository" />

