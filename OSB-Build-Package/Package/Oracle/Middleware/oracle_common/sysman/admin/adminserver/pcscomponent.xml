<?xml version="1.0" encoding="UTF-8"?>
<pcscomponent xmlns="http://www.oracle.com/ias-instance" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.oracle.com/ias-instance pcscomponent.xsd" >

<!-- The pcscomponent.xml values are used to complete the module-config.xml
     file.  We show two ways of passing a config file name.  The
     first is in an environment variable, the second as a property.
-->

<environment>
   <variable id="PCSMANAGED" value="TRUE"/>
   <variable id="PATH" value="$ORACLE_INSTANCE/$COMPONENT_TYPE/$COMPONENT_NAME/bin" append="true"/>
</environment>
</pcscomponent>

