<!ELEMENT MetricResult (TypeHeader, ValueRow+) >
<!ATTLIST MetricResult
          DATE CDATA #IMPLIED
          WARNING CDATA #IMPLIED
          TYPE ( NUMBER | STRING | TABLE | RAW | EXTERNAL | REPOSITORY_TABLE | REPOSITORY_NUMBER | REPOSITORY_STRING ) "NUMBER">

<!ELEMENT TypeHeader (ColType*) >

<!ELEMENT ValueRow (MetricValue*) >
<!ATTLIST ValueRow
          ROW_TIMESTAMP CDATA #IMPLIED>

<!ELEMENT MetricValue (#PCDATA | ValueRow+) >

<!ELEMENT ColType EMPTY>
<!ATTLIST ColType
          NAME CDATA #REQUIRED
          TYPE (NUMBER | STRING | CLOB | BLOB) "NUMBER">
