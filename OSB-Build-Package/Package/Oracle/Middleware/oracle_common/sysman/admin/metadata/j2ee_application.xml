<!DOCTYPE TargetMetadata SYSTEM "../dtds/TargetMetadata.dtd" [
<!ENTITY j2ee_application_metrics SYSTEM "./j2ee_application_metrics.xmlp">
<!ENTITY j2ee_application_response SYSTEM "./j2ee_application_response.xmlp">
<!ENTITY j2ee_application_ws_metrics SYSTEM "./j2ee_application_ws_metrics.xmlp">
<!ENTITY j2ee_application_adf_metrics SYSTEM "./j2ee_application_adf_metrics.xmlp">
<!ENTITY j2ee_application_webcenter_metrics SYSTEM "./j2ee_application_webcenter_metrics.xmlp">
<!ENTITY mds_metric SYSTEM "./mds.xmlp">
]>

<TargetMetadata META_VER='11.00000' TYPE='j2ee_application' 
                CATEGORY_PROPERTIES="VersionCategory;serverType">
 <Display>
  <Label NLSID='oracle_application_name'>Application Deployment</Label>
  <ShortName NLSID='oracle_application_name'>Application Deployment</ShortName>
  <Description NLSID='oracle_application_desc'>Application Deployment Description</Description>
</Display>

<AssocTarget ASSOCIATION_NAME="farm" ASSOC_TARGET_TYPE="oracle_ias_farm"/>

&j2ee_application_response;
&j2ee_application_metrics;
&j2ee_application_ws_metrics;
&j2ee_application_adf_metrics;
&j2ee_application_webcenter_metrics;
&mds_metric;

<InstanceProperties>

    <InstanceProperty NAME="ObjectName" CREDENTIAL="FALSE" OPTIONAL="TRUE">
      <Display>
        <Label NLSID="object_name_prop">Object Name</Label>
      </Display>
    </InstanceProperty>

    <InstanceProperty NAME="ServerNames">
       <Display>
          <Label NLSID="serverNames">Server Names</Label>
       </Display>
    </InstanceProperty>

    <InstanceProperty NAME="CanonicalPath" CREDENTIAL="FALSE" OPTIONAL="TRUE">
      <Display>
        <Label NLSID="canonical_path_prop">Canonical Path</Label>
      </Display>
    </InstanceProperty>

    <InstanceProperty NAME="OracleInstance" CREDENTIAL="FALSE" OPTIONAL="TRUE">
      <Display>
        <Label NLSID="oracle_instance_prop">Oracle Instance</Label>
      </Display>
    </InstanceProperty>

  <InstanceProperty NAME="version">
   <Display>
     <Label NLSID="j2ee_app_version_iprop">Version of Application Deployment</Label>
    </Display>
  </InstanceProperty>

  <InstanceProperty NAME="appName">
   <Display>
     <Label NLSID="j2ee_app_name_iprop">Name of the Application Deployment</Label>
    </Display>
  </InstanceProperty>

   <InstanceProperty NAME="appVersion" OPTIONAL="TRUE">
   <Display>
     <Label NLSID="j2ee_app_version_iprop">Version of Application Deployment</Label>
    </Display>
  </InstanceProperty>

 <InstanceProperty NAME="internalApp" OPTIONAL="TRUE">
   <Display>
     <Label NLSID="j2ee_internal_app_iprop">Is this an internal application?</Label>
    </Display>
  </InstanceProperty>

  <InstanceProperty NAME="VersionCategory" IS_COMPUTED="TRUE">
   <Display>
    <Label NLSID="version_cat_iprop">Version Category</Label>
   </Display>
  </InstanceProperty>

  <InstanceProperty NAME="serverType">
   <Display>
    <Label NLSID="server_type_iprop">Server Type</Label>
   </Display>
  </InstanceProperty>

<DynamicProperties NAME="ComputeVC" FORMAT="ROW" 
                   PROP_LIST="VersionCategory">
  <QueryDescriptor FETCHLET_ID="VersionRangeComputer">
    <Property NAME="Version" SCOPE="INSTANCE">version</Property>
    <Property NAME="DefaultRange" SCOPE="GLOBAL">11plus</Property>
    <Property NAME="11plus" SCOPE="GLOBAL">10.3;</Property>
  </QueryDescriptor>
</DynamicProperties>

</InstanceProperties>

</TargetMetadata>
