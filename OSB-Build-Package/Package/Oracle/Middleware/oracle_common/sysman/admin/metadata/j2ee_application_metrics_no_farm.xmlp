	<Property NAME="serviceURL" SCOPE="INSTANCE" >serviceURL</Property>
	<Property NAME="UserName" SCOPE="INSTANCE">WeblogicUserName</Property>
	<Property NAME="password"  SCOPE="INSTANCE">WeblogicPassword</Property>
