        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="service.time" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="web_module_serviceTime">Web Module - Request Processing Time (ms)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="service.throughput" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="web_module_serviceThroughput">Web Module - Requests (per minute)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="service.completed" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE">
        <Display>
          <Label NLSID="web_module_serviceCompleted">Web Module - Requests (since startup)</Label>
        </Display>
      </ColumnDescriptor>
 </TableDescriptor>
  <QueryDescriptor FETCHLET_ID="JMX">
	<Property NAME="serviceURL" SCOPE="INSTANCE" >serviceURL</Property>
	<Property NAME="UserName" SCOPE="INSTANCE">WeblogicUserName</Property>
	<Property NAME="password"  SCOPE="INSTANCE">WeblogicPassword</Property>
         <Property NAME="metric" SCOPE="GLOBAL">weblogic_j2eeserver:web_module</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">name;session.active;service.totalTime;service.time;service.throughput;service.completed</Property>
         <Property NAME="name.parameter" SCOPE="USER" OPTIONAL="TRUE">name.parameter</Property>
         <Property NAME="appName.parameter" SCOPE="INSTANCE">appName</Property>
         <Property NAME="admlMap" SCOPE="GLOBAL">
           <![CDATA[<argument type="java.util.Map"><value><item><key>appName.parameter</key><value type="java.lang.String">%appName.parameter%</value></item></value></argument>]]>
         </Property>
  </QueryDescriptor>
