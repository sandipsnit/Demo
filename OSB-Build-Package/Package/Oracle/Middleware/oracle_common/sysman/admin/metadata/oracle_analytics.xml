<?xml version="1.0" ?>
<!DOCTYPE TargetMetadata SYSTEM "../dtds/TargetMetadata.dtd" [
]>

<TargetMetadata META_VER="2.3" TYPE="oracle_analytics" CATEGORY_PROPERTIES="VersionCategory">
   <Display>
      <Label NLSID="WEBLOGIC_LABEL">Analytics</Label>
      <ShortName NLSID="WEBLOGIC_NAME">Analytics</ShortName>
      <Description NLSID="WEBLOGIC_DESC">Analytics</Description>
   </Display>

   <Metric NAME="Response" TYPE="TABLE">
      <Display>
         <Label NLSID="WEBLOGIC_J2EESERVER_RESPONSE">Response</Label>
      </Display>
      <TableDescriptor>
         <ColumnDescriptor NAME="Status" TYPE="NUMBER" IS_KEY="FALSE">
            <Display>
               <Label NLSID="WEBLOGICJ2EE_SERVER_STATUS_LABEL">Status</Label>
               <Description NLSID="WEBLOGICJ2EE_SERVER_STATUS_DESC">Current Status of the Server</Description>
            </Display>
         </ColumnDescriptor>
      </TableDescriptor>
      <QueryDescriptor FETCHLET_ID="JMX">
        <ValidIf>
          <CategoryProp NAME="VersionCategory" CHOICES="11plus" />
        </ValidIf>
        <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
        <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
        <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
        <Property NAME="metric" SCOPE="GLOBAL">analytics:Response</Property>
        <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
        <Property NAME="ServerNames.parameter" SCOPE="INSTANCE">ServerNames</Property>
        <Property NAME="useCache" SCOPE="GLOBAL">false</Property>
        <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
        <Property NAME="columnOrder" SCOPE="GLOBAL">Status</Property>
         <Property NAME="admlMap" SCOPE="GLOBAL">
           <![CDATA[<argument type="java.util.Map"><value><item><key>ServerNames.parameter</key><value type="java.lang.String">%ServerNames.parameter%</value></item></value></argument>]]>
         </Property>         
      </QueryDescriptor>
   </Metric>
   <InstanceProperties>
      <InstanceProperty NAME="MachineName">
         <ValidIf>
           <CategoryProp NAME="VersionCategory" CHOICES="7x;8x;9Plus" />
         </ValidIf>
         <Display>
            <Label NLSID="MACHINENAME_LABEL">Machine Name</Label>
            <ShortName NLSID="MACHINENAME_NAME">Machine Name</ShortName>
         </Display>
      </InstanceProperty>
      <InstanceProperty NAME="Port">
         <ValidIf>
           <CategoryProp NAME="VersionCategory" CHOICES="7x;8x;9Plus" />
         </ValidIf>
         <Display>
            <Label NLSID="PORT_LABEL">Port Number</Label>
            <ShortName NLSID="PORT_NAME">Port Number</ShortName>
         </Display>
      </InstanceProperty>
      <InstanceProperty NAME="UserName">
         <ValidIf>
           <CategoryProp NAME="VersionCategory" CHOICES="7x;8x;9Plus" />
         </ValidIf>
         <Display>
            <Label NLSID="USERNAME_LABEL">Username</Label>
            <ShortName NLSID="USERNAME_NAME">Username</ShortName>
         </Display>
      </InstanceProperty>
      <InstanceProperty NAME="password" CREDENTIAL="TRUE">
         <ValidIf>
           <CategoryProp NAME="VersionCategory" CHOICES="7x;8x;9Plus" />
         </ValidIf>
         <Display>
            <Label NLSID="PASSWORD_LABEL">Password</Label>
            <ShortName NLSID="PASSWORD_NAME">Password</ShortName>
         </Display>
      </InstanceProperty>
      <InstanceProperty NAME="KeyStoreFile" OPTIONAL="TRUE">
         <Display>
            <Label NLSID="KEYSTOREFILE">Keystore Filename</Label>
         </Display>
      </InstanceProperty>
      <InstanceProperty NAME="version" OPTIONAL="TRUE">
         <Display>
            <Label NLSID="VERSION">Version</Label>
         </Display>
      </InstanceProperty>     
      <InstanceProperty NAME="jarpath" OPTIONAL="TRUE">
         <Display>
            <Label NLSID="JARPATH">Jar Path</Label>
         </Display>
      </InstanceProperty>     
      <InstanceProperty NAME="ServerNames">
         <Display>
            <Label NLSID="serverNames">Server Names</Label>
         </Display>
      </InstanceProperty>     
      <DynamicProperties NAME="ComputeVC" FORMAT="ROW"
                   PROP_LIST="VersionCategory">
        <QueryDescriptor FETCHLET_ID="VersionRangeComputer">
          <Property NAME="Version" SCOPE="INSTANCE" OPTIONAL="TRUE">version</Property>
          <Property NAME="DefaultRange" SCOPE="GLOBAL">8x</Property>
          <Property NAME="7x" SCOPE="GLOBAL">7;</Property>
          <Property NAME="8x" SCOPE="GLOBAL">8;</Property>          
          <Property NAME="9Plus" SCOPE="GLOBAL">9;10;</Property>
          <Property NAME="11plus" SCOPE="GLOBAL">10.3;</Property>
        </QueryDescriptor>
      </DynamicProperties>
   </InstanceProperties>
</TargetMetadata>
