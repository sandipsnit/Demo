      <Metric NAME="UsageByDashBoardInLast7Days" TYPE="TABLE">
      <Display>
         <Label NLSID="USAGE_BY_DASHBOARD_IN_LAST_7_DAYS">Dashboard Usage in Last 7 Days</Label>
      </Display>
      <TableDescriptor>
         <ColumnDescriptor NAME="dashboard" TYPE="STRING" IS_KEY="TRUE">
            <Display>
               <Label NLSID="DASHBOARD">Dashboard</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="user_name" TYPE="STRING" IS_KEY="TRUE">
            <Display>
                <Label NLSID="USER_NAME">User Name</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="count" TYPE="NUMBER" IS_KEY="FALSE">
            <Display>
                <Label NLSID="COUNT">Count</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="last_accessed_on" TYPE="STRING" IS_KEY="FALSE">
            <Display>
                <Label NLSID="LAST_ACCESSED_ON">Last Accessed On</Label>
            </Display>    
         </ColumnDescriptor>
      </TableDescriptor>
      <QueryDescriptor FETCHLET_ID="JDBCSQL">
         <Property NAME="ConnInfo" SCOPE="INSTANCE" OPTIONAL="TRUE">DBConnectionString</Property>
         <Property NAME="Class" SCOPE="INSTANCE" OPTIONAL="TRUE">DBClassString</Property>
         <Property NAME="STATEMENT" SCOPE="GLOBAL">
            <![CDATA[
                
                select SAW_DASHBOARD, USER_NAME, count(diff), max(START_TS) from
                (
                    select
                        SAW_DASHBOARD, USER_NAME,
                        case
                        concat(LAG(USER_NAME, 1, USER_NAME)  OVER(ORDER BY SAW_DASHBOARD, USER_NAME, START_TS desc), 
                        LAG(SAW_DASHBOARD, 1, SAW_DASHBOARD)  OVER(ORDER BY SAW_DASHBOARD, USER_NAME, START_TS desc)) when concat(USER_NAME, SAW_DASHBOARD) then
                        (START_TS - LAG(START_TS, 1, (START_TS + 10 / (24 * 60)))  OVER(ORDER BY SAW_DASHBOARD, USER_NAME, START_TS desc)) * 24 * 60
                        else -10 end diff, START_TS
                        from S_NQ_ACCT WHERE (START_TS > SYSDATE - 7) AND SAW_DASHBOARD IS NOT NULL
                )
                where diff <= -10
                group by SAW_DASHBOARD, USER_NAME 
            ]]>
         </Property>
         <Property NAME="CONNECT_PROP_user" SCOPE="INSTANCE" OPTIONAL="TRUE">DBUserName</Property>
         <Property NAME="CONNECT_PROP_password" SCOPE="INSTANCE" OPTIONAL="TRUE">DBPassword</Property>
      </QueryDescriptor>
   </Metric>


