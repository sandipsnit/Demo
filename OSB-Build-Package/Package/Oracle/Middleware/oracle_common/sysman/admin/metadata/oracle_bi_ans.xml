      <QueryDescriptor FETCHLET_ID="JDBCSQL">
         <Property NAME="ConnInfo" SCOPE="INSTANCE" OPTIONAL="TRUE">DBConnectionString</Property>
         <Property NAME="Class" SCOPE="INSTANCE" OPTIONAL="TRUE">DBClassString</Property>
         <Property NAME="STATEMENT" SCOPE="GLOBAL">
            <![CDATA[
                SELECT * FROM 
(SELECT 
    USER_NAME, 
    (SUM(CUM_DB_TIME_SEC) + SUM(TOTAL_TIME_SEC) + SUM(COMPILE_TIME_SEC)) OVERALL_TIME, 
    SUM(CUM_DB_TIME_SEC) DTS, 
    SUM(COMPILE_TIME_SEC) CTS, 
    (SELECT COUNT(*) FROM S_NQ_ACCT T WHERE T.USER_NAME = S.USER_NAME AND SUCCESS_FLG > 0 AND 
          (T.SAW_DASHBOARD IS NOT NULL) AND (T.END_TS > SYSDATE - 7)) AS FAILED_REQUESTS, 
    COUNT(*) AS TOTAL_REQUESTS
FROM S_NQ_ACCT S 
WHERE (S.USER_NAME IS NOT NULL) 
AND (END_TS > SYSDATE - 7) 
GROUP BY USER_NAME 
ORDER BY OVERALL_TIME DESC) WHERE ROWNUM <= 20
		]]>
         </Property>
         <Property NAME="CONNECT_PROP_user" SCOPE="INSTANCE" OPTIONAL="TRUE">DBUserName</Property>
         <Property NAME="CONNECT_PROP_password" SCOPE="INSTANCE" OPTIONAL="TRUE">DBPassword</Property>
      </QueryDescriptor>
   </Metric>
