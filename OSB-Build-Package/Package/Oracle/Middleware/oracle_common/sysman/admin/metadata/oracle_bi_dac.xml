   <Metric NAME="ETL Performance" TYPE="TABLE">
      <Display>
         <Label NLSID="oracle_bi_dac_etlTasks">ETL Performance</Label>
      </Display>
      <TableDescriptor>
         <ColumnDescriptor NAME="aggregateTotalTasks" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE" HELP="NO_HELP">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_aggrTotalTasks">Total tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="aggregateFailedTasks" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE" HELP="NO_HELP">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_aggrFailedTasks">Total failed tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="aggregateCompletedTasks" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE" HELP="NO_HELP">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_aggrCompletedTasks">Total completed tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="queuedTasks" TYPE="NUMBER" IS_KEY="FALSE">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_queuedTasks">Queued tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="runningTasks" TYPE="NUMBER" IS_KEY="FALSE">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_runningTasks">Running tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="runnableTasks" TYPE="NUMBER" IS_KEY="FALSE">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_runnableTasks">Runnable tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="totalTasks" TYPE="NUMBER" IS_KEY="FALSE"
                  COMPUTE_EXPR="(aggregateTotalTasks - _aggregateTotalTasks) > 0 ? (aggregateTotalTasks - _aggregateTotalTasks) : 0">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_totalTasks">Total tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="failedTasks" TYPE="NUMBER" IS_KEY="FALSE"
                  COMPUTE_EXPR="(aggregateFailedTasks - _aggregateFailedTasks) > 0 ? (aggregateFailedTasks - _aggregateFailedTasks) : 0">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_failedTasks">Failed tasks</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="completedTasks" TYPE="NUMBER" IS_KEY="FALSE"
                  COMPUTE_EXPR="(aggregateCompletedTasks - _aggregateCompletedTasks) > 0 ? (aggregateCompletedTasks - _aggregateCompletedTasks) : 0">
            <Display FOR_SUMMARY_UI="TRUE">
               <Label NLSID="oracle_bi_dac_completedTasks">Completed tasks</Label>
            </Display>
         </ColumnDescriptor>
      </TableDescriptor>
      <QueryDescriptor FETCHLET_ID="JDBCSQL">
         <Property NAME="ConnInfo" SCOPE="INSTANCE">DBConnectionString</Property>
         <Property NAME="Class" SCOPE="INSTANCE">DBClassString</Property>
         <Property NAME="STATEMENT" SCOPE="GLOBAL">
            <![CDATA[
                 select max(c1), max(c2), max(c3), max(c4), max(c5), max(c6) from 
		(
                    select count(*) as c1, 0 as c2, 0 as c3, 0 as c4, 0 as c5, 0 as c6 from W_ETL_RUN_STEP
                        UNION ALL
		    select 0 as c1, count(*) as c2, 0 as c3, 0 as c4, 0 as c5, 0 as c6 from W_ETL_RUN_STEP where STATUS = 'Failed'
                        UNION ALL
		    select 0 as c1, 0 as c2, count(*) as c3, 0 as c4, 0 as c5, 0 as c6 from W_ETL_RUN_STEP where STATUS = 'Completed'
                        UNION ALL
		    select 0 as c1, 0 as c2, 0 as c3, count(*) as c4, 0 as c5, 0 as c6 from W_ETL_RUN_STEP where STATUS = 'Queued'
                        UNION ALL
		    select 0 as c1, 0 as c2, 0 as c3, 0 as c4, count(*) as c5, 0 as c6 from W_ETL_RUN_STEP where STATUS = 'Running'
                        UNION ALL
		    select 0 as c1, 0 as c2, 0 as c3, 0 as c4, 0 as c5, count(*) as c6 from W_ETL_RUN_STEP where STATUS = 'Runnable'
                ) a
		]]>
         </Property>
         <Property NAME="CONNECT_PROP_user" SCOPE="INSTANCE">DBUserName</Property>
         <Property NAME="CONNECT_PROP_password" SCOPE="INSTANCE">DBPassword</Property>
      </QueryDescriptor>
   </Metric>
