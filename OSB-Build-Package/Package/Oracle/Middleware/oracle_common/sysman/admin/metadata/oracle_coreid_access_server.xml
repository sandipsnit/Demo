         <JoinTables NAME="TotalAndSuccessAuth">
			<Table NAME="aaaAuthenticationsSuccess" />
			<Table NAME="TotalAuthentications" />
			<Column NAME="authenSuccess" TABLE_NAME="aaaAuthenticationsSuccess"
			               COLUMN_NAME="AuthenticationSuccess" />
			<Column NAME="authenSuccessRate" TABLE_NAME="aaaAuthenticationsSuccess"
			               COLUMN_NAME="AuthenticationSuccessRate" />
            <Column NAME="totalAuthentication" TABLE_NAME="TotalAuthentications" 
			           COLUMN_NAME="AggregateAuthentication"/>
			<Column NAME="totalAuthenticationRate" TABLE_NAME="TotalAuthentications" 
			           COLUMN_NAME="AggregateAuthenticationRate"/>
		 </JoinTables>

        <GroupBy NAME="PercentSuccessAuthentications" FROM_TABLE="TotalAndSuccessAuth">
		      <AggregateColumn  NAME="authenSuccess"
                          COLUMN_NAME="authenSuccess"
                          OPERATOR="AVG"/>
			   <AggregateColumn  NAME="authenSuccessRate"
                          COLUMN_NAME="authenSuccessRate"
                          OPERATOR="AVG"/>
			   
			  <AggregateColumn  NAME="totalAuthentication"
                          COLUMN_NAME="totalAuthentication"
                          OPERATOR="AVG"/>

			  <AggregateColumn  NAME="totalAuthenticationRate"
                          COLUMN_NAME="totalAuthenticationRate"
                          OPERATOR="AVG"/>

             <ComputeColumn    NAME="PercentSucAuthentication"
                          EXPR="(authenSuccess*100)/totalAuthentication" /> 

			 <ComputeColumn    NAME="PercentSucAuthenticationRate"
                          EXPR="totalAuthenticationRate == 0 ? 0 : (authenSuccessRate*100)/totalAuthenticationRate" /> 
        </GroupBy>  

      </ExecutionDescriptor>
	</Metric> 

