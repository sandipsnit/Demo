	  <Metric NAME="TotalAuthentications" TYPE="TABLE" USAGE_TYPE="HIDDEN" >    
		<Display>
		  <Label NLSID="total_authen_nlsid">Total Authentications</Label>
		  <Description NLSID="total_authen_desc_nlsid">Total authentications</Description>
		</Display>    
		<TableDescriptor>
			<ColumnDescriptor NAME="AggregateAuthentication" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="TotalAuthenticNLS">Total Authentications</Label>
			  </Display>
			</ColumnDescriptor>
			<ColumnDescriptor NAME="AggregateAuthenticationRate" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="TotalAuthenticRateNLS">Total Authentications(since last collection)</Label>
			  </Display>
			</ColumnDescriptor>
		</TableDescriptor>
		<ExecutionDescriptor>
         <GetTable NAME="aaaAuthenticationsSuccess" USE_CACHE="FALSE"/>
         <GetTable NAME="aaaAuthenticationsDenied" USE_CACHE="FALSE"/>
		 <GetView NAME="AuthenticSuccessView" FROM_TABLE="aaaAuthenticationsSuccess">
		    <Column NAME="authentication" COLUMN_NAME="AuthenticationSuccess"
			        TABLE_NAME="aaaAuthenticationsSuccess"/>
			<Column NAME="authenticationRate" COLUMN_NAME="AuthenticationSuccessRate" 
			          TABLE_NAME="aaaAuthenticationsSuccess"/>
         </GetView>
		 <GetView NAME="AuthenticFailView" FROM_TABLE="aaaAuthenticationsDenied">
		    <Column NAME="authentication" COLUMN_NAME="AuthDenied" TABLE_NAME="aaaAuthenticationsDenied"/>
			<Column NAME="authenticationRate" COLUMN_NAME="AuthDeniedRate" 
			          TABLE_NAME="aaaAuthenticationsDenied"/>
         </GetView>

         <Union NAME="unionAuthentication">     
             <Table NAME="AuthenticSuccessView" />
             <Table NAME="AuthenticFailView" />
        </Union>
        <GroupBy NAME="TotalAuthentications" FROM_TABLE="unionAuthentication">
             <AggregateColumn  NAME="AggregateAuthentication"
                          COLUMN_NAME ="authentication"
                          OPERATOR="SUM" /> 
			  <AggregateColumn  NAME="AggregateAuthenticationRate"
                          COLUMN_NAME ="authenticationRate"
                          OPERATOR="SUM" /> 
        </GroupBy>  
      </ExecutionDescriptor>
	</Metric> 
	
	<Metric NAME="TotalAuthorization" TYPE="TABLE" USAGE_TYPE="HIDDEN" >    
		<Display>
		  <Label NLSID="total_author_nlsid">Total Authorizations</Label>
		  <Description NLSID="total_author_desc_nlsid">Total authorization</Description>
		</Display>    
		<TableDescriptor>
			<ColumnDescriptor NAME="AggregateAuthorization" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="TotalAthorNLS">Total Authorizations</Label>
			  </Display>
			</ColumnDescriptor>
			<ColumnDescriptor NAME="AggregateAuthorizationRate" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="TotalAthorRateNLS">Total Authorizations(last collected)</Label>
			  </Display>
			</ColumnDescriptor>
		</TableDescriptor>
		<ExecutionDescriptor>
         <GetTable NAME="aaaAuthorizationsSuccess" USE_CACHE="FALSE"/>
         <GetTable NAME="aaaAuthorizationsDenied" USE_CACHE="FALSE"/>
		 <GetView NAME="AuthorizationSuccessView" FROM_TABLE="aaaAuthorizationsSuccess">
		    <Column NAME="authorization" COLUMN_NAME="AuthorizationSuccess" TABLE_NAME="aaaAuthorizationsSuccess"/>
			<Column NAME="authorizationRate" COLUMN_NAME="AuthorizationSuccessRate" TABLE_NAME="aaaAuthorizationsSuccess"/>
         </GetView>
		 <GetView NAME="AuthorizationFailView" FROM_TABLE="aaaAuthorizationsDenied">
		    <Column NAME="authorization" COLUMN_NAME="AuthorizationDenied" TABLE_NAME="aaaAuthorizationsDenied"/>
			<Column NAME="authorizationRate" COLUMN_NAME="AuthorizationDeniedRate" TABLE_NAME="aaaAuthorizationsDenied"/>
         </GetView>

         <Union NAME="unionAuthorization">     
             <Table NAME="AuthorizationSuccessView" />
             <Table NAME="AuthorizationFailView" />
        </Union>
        <GroupBy NAME="TotalAuthorization" FROM_TABLE="unionAuthorization">
             <AggregateColumn  NAME="AggregateAuthorization"
                          COLUMN_NAME ="authorization"
                          OPERATOR="SUM" />
			 <AggregateColumn  NAME="AggregateAuthorizationRate"
                          COLUMN_NAME ="authorizationRate"
                          OPERATOR="SUM" />
        </GroupBy>  
      </ExecutionDescriptor>
	</Metric>  

	<Metric NAME="RequestProcInfo" TYPE="TABLE" >    
		<Display>
		  <Label NLSID="request_nlsid">Request Processed by Access Server</Label>
		  <Description NLSID="request_desc_nlsid">Request processed by Access Server</Description>
		</Display>    
		<TableDescriptor>
			<ColumnDescriptor NAME="TotalRequest" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="TotalRequestNLS">Total Request Processed</Label>
			  </Display>
			</ColumnDescriptor>
			<ColumnDescriptor NAME="LastRequestProcessed" TYPE="NUMBER" IS_KEY="FALSE">
			  <Display>
				<Label NLSID="LastRequestProcessedNLS">Request Processed (since last collection)</Label>
			  </Display>
			</ColumnDescriptor>
		</TableDescriptor>
		<ExecutionDescriptor>
         <GetTable NAME="TotalAuthentications" USE_CACHE="FALSE"/>
         <GetTable NAME="TotalAuthorization" USE_CACHE="FALSE"/>
		 <GetView NAME="TotalAuthenticationsView" FROM_TABLE="TotalAuthentications">
		    <Column NAME="totalRequest" COLUMN_NAME="AggregateAuthentication" TABLE_NAME="TotalAuthentications"/>
			<Column NAME="lastRequest" COLUMN_NAME="AggregateAuthenticationRate" TABLE_NAME="TotalAuthentications"/>
         </GetView>
		 <GetView NAME="TotalAuthorizationView" FROM_TABLE="TotalAuthorization">
		    <Column NAME="totalRequest" COLUMN_NAME="AggregateAuthorization" TABLE_NAME="TotalAuthorization"/>
			<Column NAME="lastRequest" COLUMN_NAME="AggregateAuthorizationRate" TABLE_NAME="TotalAuthorization"/>
         </GetView>

         <Union NAME="unionRequest">     
             <Table NAME="TotalAuthenticationsView" />
             <Table NAME="TotalAuthenticationsView" />
        </Union>
        <GroupBy NAME="RequestProcInfo" FROM_TABLE="unionRequest">
             <AggregateColumn  NAME="TotalRequest"
                          COLUMN_NAME ="totalRequest"
                          OPERATOR="SUM" />
			 <AggregateColumn  NAME="LastRequestProcessed"
                          COLUMN_NAME ="lastRequest"
                          OPERATOR="SUM" />
        </GroupBy>  
      </ExecutionDescriptor>
	</Metric>  

