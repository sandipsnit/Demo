    <Metric NAME="Logins" TYPE="TABLE">
        <ValidIf>
            <CategoryProp NAME="VersionCategory" CHOICES="11plus" />
        </ValidIf>
        <Display>
            <Label NLSID="Logins">Login Metrics Summary</Label>
        </Display>
        <TableDescriptor>      
             <ColumnDescriptor NAME="Login_Success.persec" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="LoginSuccess_persec">Number of Successful Logins per sec since the server startup</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed.persec" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="LoginFailed_persec">Number of Failed Logins per sec since the server startup</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Blocked_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Blocked_delta">
                <Display>
                    <Label NLSID="LoginBlocked">Number of Logins Blocked during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Success_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Success_delta">
                <Display>
                    <Label NLSID="LoginSuccess">Number of Successful Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Total_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Total_delta">
                <Display>
                    <Label NLSID="LoginTotal">Number of Total Logins attempted during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Failed_delta">
                <Display>
                    <Label NLSID="LoginFailed">Number of Logins  Failed during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Challenged_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Challenged_delta">
                <Display>
                    <Label NLSID="LoginChallenged">Number of Logins Challagened during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
             <ColumnDescriptor NAME="Login_Success.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Success.percent">
                <Display>
                    <Label NLSID="Login_Success_percent">Percentage of Successful Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Failed.percent">
                <Display>
                    <Label NLSID="Login_Failed_percent">Percentage of Failed Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Blocked.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Blocked.percent">
                <Display>
                    <Label NLSID="Login_Blocked_percent">Percentage of Blocked Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Challenged.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Challenged.percent">
                <Display>
                    <Label NLSID="Login_Challenged_percent">Percentage of Challagened Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
        </TableDescriptor>
        <QueryDescriptor FETCHLET_ID="JMX">
            <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
            <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
            <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
            <Property NAME="metric" SCOPE="GLOBAL">oracle_oaam:OAAM_Login_Metrics</Property>
            <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
            <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
            <Property NAME="MetricSource" SCOPE="GLOBAL">Weblogic</Property>
            <Property NAME="columnOrder" SCOPE="GLOBAL">Login_Success.persec;Login_Failed.persec;Login_Blocked_delta;Login_Success_delta;Login_Total_delta;Login_Failed_delta;Login_Challenged_delta;Login_Success.percent;Login_Failed.percent;Login_Blocked.percent;Login_Challenged.percent</Property>
        </QueryDescriptor>
    </Metric>
