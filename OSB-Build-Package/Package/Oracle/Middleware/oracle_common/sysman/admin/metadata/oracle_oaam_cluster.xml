            <!DOCTYPE TargetMetadata SYSTEM "../dtds/TargetMetadata.dtd" [
]>
<TargetMetadata META_VER="11.00000" TYPE="oracle_oaam_cluster" RESOURCE_BUNDLE_PACKAGE="oracle.sysman.emas.rsc.gen.iam"  CATEGORY_PROPERTIES="VersionCategory">
    <Display>
        <Label NLSID="ORACLE_ADAPTIVE_ACCESS_MANAGER_NAME">Oracle Adaptive Access Manager Cluster</Label>
        <ShortName NLSID="ORACLE_ADAPTIVE_ACCESS_MANAGER_NAME">Oracle Adaptive Access Manager Cluster</ShortName>
        <Description NLSID="ORACLE_ADAPTIVE_ACCESS_MANAGER_DESC">Oracle Adaptive Access Manager Cluster</Description>
    </Display>
     <Metric NAME="Response" TYPE="TABLE">
    <Display>
      <Label NLSID="oracle_oaam_cluster_response">Response</Label>
    </Display>
    <TableDescriptor>
      <ColumnDescriptor NAME="Status" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="oracle_oaam_cluster_status">Status</Label>
        </Display>
      </ColumnDescriptor>
    </TableDescriptor>
    <QueryDescriptor FETCHLET_ID="JMX">    
      <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
      <Property NAME="UserName" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">UserName</Property>
      <Property NAME="password" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">password</Property>
      <Property NAME="metric" SCOPE="GLOBAL">domain_j2ee_application:deployment_status</Property>
      <Property NAME="MetricSource" SCOPE="GLOBAL">Weblogic</Property>
      <Property NAME="ServerNames" SCOPE="GLOBAL">AdminServer</Property>
      <Property NAME="appName.parameter" SCOPE="INSTANCE">appName</Property>
      <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
      <Property NAME="columnOrder" SCOPE="GLOBAL">Status</Property>
      <Property NAME="useCache" SCOPE="GLOBAL">false</Property>
      <Property NAME="admlMap" SCOPE="GLOBAL">
        <![CDATA[<argument type="java.util.Map"><value><item><key>appName.parameter</key><value type="java.lang.String">%appName.parameter%</value></item></value></argument>]]>
         </Property>
    </QueryDescriptor>   
  </Metric>
    <Metric NAME="Logins" TYPE="TABLE">
        <ValidIf>
            <CategoryProp NAME="VersionCategory" CHOICES="11plus" />
        </ValidIf>
        <Display>
            <Label NLSID="Logins">Login Metrics Summary</Label>
        </Display>
        <TableDescriptor>      
            <ColumnDescriptor NAME="Login_Blocked_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Blocked_delta">
                <Display>
                    <Label NLSID="LoginBlocked">Number of Logins Blocked during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Success_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Success_delta">
                <Display>
                    <Label NLSID="LoginSuccess">Number of Successful Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Total_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Total_delta">
                <Display>
                    <Label NLSID="LoginTotal">Number of Total Logins attempted during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Failed_delta">
                <Display>
                    <Label NLSID="LoginFailed">Number of Logins Failed during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Challenged_delta" TYPE="NUMBER"  COLUMN_NAME="Login_Challenged_delta">
                <Display>
                    <Label NLSID="LoginChallenged">Number of Logins Challagened during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Success.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Success.percent">
                <Display>
                    <Label NLSID="Login_Success_percent">Percentage of Successful Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Failed.percent">
                <Display>
                    <Label NLSID="Login_Failed_percent">Percentage of Failed Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Blocked.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Blocked.percent">
                <Display>
                    <Label NLSID="Login_Blocked_percent">Percentage of Blocked Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Challenged.percent" TYPE="NUMBER"  COLUMN_NAME="Login_Challenged.percent">
                <Display>
                    <Label NLSID="Login_Challenged_percent">Percentage of Challagened Logins during the last 5 Minute collection interval</Label>
                </Display>
            </ColumnDescriptor>
        </TableDescriptor>
        <QueryDescriptor FETCHLET_ID="JMX">
            <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
            <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
            <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
            <Property NAME="metric" SCOPE="GLOBAL">domain_oracle_oaam:OAAM_Login_Metrics_delta</Property>
      <Property NAME="ServerNames" SCOPE="GLOBAL">AdminServer</Property>
            <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
            <Property NAME="MetricSource" SCOPE="GLOBAL">Weblogic</Property>
            <Property NAME="columnOrder" SCOPE="GLOBAL">Login_Blocked_delta;Login_Success_delta;Login_Total_delta;Login_Failed_delta;Login_Challenged_delta;Login_Success.percent;Login_Failed.percent;Login_Blocked.percent;Login_Challenged.percent</Property>
        </QueryDescriptor>
    </Metric>
    <Metric NAME="Logins_rate" TYPE="TABLE">
        <ValidIf>
            <CategoryProp NAME="VersionCategory" CHOICES="11plus" />
        </ValidIf>
        <Display>
            <Label NLSID="Logins_rate">Login Metrics Rate Summary</Label>
        </Display>
        <TableDescriptor>      
            <ColumnDescriptor NAME="Login_Success.persec" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="LoginSuccess_persec">Number of Successful Logins per sec</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="Login_Failed.persec" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="LoginFailed_persec">Number of Failed Logins per sec</Label>
                </Display>
            </ColumnDescriptor>
        </TableDescriptor>
        <QueryDescriptor FETCHLET_ID="JMX">
            <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
            <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
            <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
            <Property NAME="metric" SCOPE="GLOBAL">domain_oracle_oaam:OAAM_Login_Metrics_rollup_rate</Property>
      <Property NAME="ServerNames" SCOPE="GLOBAL">AdminServer</Property>
            <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
            <Property NAME="MetricSource" SCOPE="GLOBAL">Weblogic</Property>
            <Property NAME="columnOrder" SCOPE="GLOBAL">Login_Success.persec;Login_Failed.persec</Property>
        </QueryDescriptor>
    </Metric>
     <Metric NAME="JDBCDataStore" TYPE="TABLE">
        <ValidIf>
            <CategoryProp NAME="VersionCategory" CHOICES="11plus" />
        </ValidIf>
        <Display>
            <Label NLSID="JDBCDataStore">Oracle Adaptive Access Manager Datastore Information</Label>
        </Display>
        <TableDescriptor> 
            <ColumnDescriptor NAME="JDBCURL" TYPE="STRING" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="JDBCURL">JDBC URL</Label>
                </Display>
            </ColumnDescriptor>
        </TableDescriptor>
         <QueryDescriptor FETCHLET_ID="JMX">
            <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
            <Property NAME="UserName" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">UserName</Property>
            <Property NAME="password" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">password</Property>
            <Property NAME="metric" SCOPE="GLOBAL">domain_oracle_oaam:OAAM_Datasource</Property>
            <Property NAME="ServerNames" SCOPE="GLOBAL">AdminServer</Property>
            <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
            <Property NAME="MetricSource" SCOPE="GLOBAL">Weblogic</Property>
            <Property NAME="columnOrder" SCOPE="GLOBAL">JDBCURL</Property>
          
        </QueryDescriptor>
    </Metric>

    <InstanceProperties>
        <InstanceProperty NAME="version" CREDENTIAL="FALSE" OPTIONAL="FALSE">      
            <Display>
                <Label NLSID="version_iprop">Version</Label>
            </Display>
        </InstanceProperty>
        <InstanceProperty NAME="VersionCategory" IS_COMPUTED="TRUE">
            <Display>
                <Label NLSID="version_cat_iprop">Version Category</Label>
            </Display>
        </InstanceProperty>
        <!--   <InstanceProperty NAME="ServerNames">
            <Display>
                <Label NLSID="serverNameLabel">ServerNames</Label>
            </Display>
        </InstanceProperty> -->
        <InstanceProperty NAME="appName">
            <Display>
                <Label NLSID="appNameLabel">appName</Label>
            </Display>
        </InstanceProperty>
         <DynamicProperties NAME="ComputeVC" FORMAT="ROW" PROP_LIST="VersionCategory">
            <QueryDescriptor FETCHLET_ID="VersionRangeComputer">
                <Property NAME="Version" SCOPE="INSTANCE">version</Property>       
                <Property NAME="11plus" SCOPE="GLOBAL">11.1.1.;</Property>
            </QueryDescriptor>
        </DynamicProperties>
    </InstanceProperties>
</TargetMetadata>


