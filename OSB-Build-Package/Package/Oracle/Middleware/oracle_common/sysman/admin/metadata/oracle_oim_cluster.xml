  <Metric NAME="Reconciliations" TYPE="TABLE" USAGE_TYPE="VIEW_COLLECT">
    <Display>
      <Label NLSID="OIM_CLUSTER_RECON_METRIC_LABEL">Reconciliations (last 24 hrs)</Label>
    </Display>
    <TableDescriptor>
      <ColumnDescriptor NAME="jobsStarted" TYPE="NUMBER" IS_KEY="FALSE"
                        TRANSIENT="FALSE">
        <Display>
          <Label NLSID="OIM_CLUSTER_JOBS_STARTED_LABEL">Jobs Started</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="jobsCompleted" TYPE="NUMBER" IS_KEY="FALSE"
                        TRANSIENT="FALSE">
        <Display>
          <Label NLSID="OIM_CLUSTER_JOBS_FINISHED_LABEL">Jobs Completed</Label>
        </Display>
      </ColumnDescriptor>
    </TableDescriptor>
    <QueryDescriptor FETCHLET_ID="JDBCSQL">
      <Property NAME="STATEMENT" SCOPE="GLOBAL">
        <![CDATA[
             select started, completed
               from (select count(rj_key) as started
                     from RECON_JOBS
                     where RJ_JOB_STATUS='Job Started' and (sysdate - RJ_START_TIME)*24 <= 24),
                    (select count(rj_key) as completed
                     from RECON_JOBS 
                     where RJ_JOB_STATUS='Event Creation Completed' and (sysdate - RJ_START_TIME)*24 <= 24)                        
        ]]>
      </Property>
      <Property NAME="ConnInfo" SCOPE="INSTANCE">jdbcurl</Property>
      <Property NAME="Class" SCOPE="INSTANCE">jdbcdriver</Property>
      <Property NAME="CONNECT_PROP_user" SCOPE="INSTANCE">oimdb_schema_user</Property>
      <Property NAME="CONNECT_PROP_password" SCOPE="INSTANCE">oimdb_password</Property>
    </QueryDescriptor>
  </Metric-->
  
