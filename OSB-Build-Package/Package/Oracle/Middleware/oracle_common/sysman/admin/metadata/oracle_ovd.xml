    <Metric NAME="ServerWideMetrics" TYPE="TABLE" COLLECT_WHEN_NO_REPOS="TRUE" NUM_CACHE_VALUES="10" FORCE_CACHE="TRUE">
        <Display>
            <Label NLSID="ServerWideMetrics">Server wide Connection Metrics</Label>
        </Display>
        <TableDescriptor>
            <ColumnDescriptor NAME="OpCnt" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="ServerWideMetrics_OpCnt">Total No of Operations</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="OpenConnections_raw" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE">
                <Display>
                    <Label NLSID="ServerWideMetrics_OpenConnections">Total No of Open Connections</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="NoofUsers" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="ServerWideMetrics_NoOfUsers">Total No of Users connected currently</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="NoofIPs" TYPE="NUMBER" IS_KEY="FALSE">
                <Display>
                    <Label NLSID="ServerWideMetrics_NoOfIPS">Total No of IPs connected currently</Label>
                </Display>
            </ColumnDescriptor>
            <ColumnDescriptor NAME="OpenConnections" TYPE="NUMBER"  COLUMN_NAME="OpenConnections"
                COMPUTE_EXPR="(OpenConnections_raw >= 0) ? OpenConnections_raw : 0">
                <Display>
                    <Label NLSID="ServerWideMetrics_OpenConnections">Total No of Open Connections</Label>
                </Display>
            </ColumnDescriptor>
        </TableDescriptor>
        <ExecutionDescriptor>

            <GetTable NAME="AdaptersInfo"  USE_CACHE="TRUE_IF_COLLECT"/>
            <GetTable NAME="getUsers"  USE_CACHE="TRUE_IF_COLLECT"/>
            <GetTable NAME="getIPs"  USE_CACHE="TRUE_IF_COLLECT"/>

            <GroupBy NAME="result2" FROM_TABLE="getIPs">
                <AggregateColumn  NAME="NoofIPs"
                    COLUMN_NAME ="name"
                    OPERATOR="COUNT" />
            </GroupBy>
            <GroupBy NAME="result1" FROM_TABLE="getUsers">
                <AggregateColumn  NAME="OpenConnections_raw"
                    COLUMN_NAME ="currentCons"
                    OPERATOR="SUM" />
                <AggregateColumn  NAME="NoofUsers"
                    COLUMN_NAME ="name"
                    OPERATOR="COUNT" />
            </GroupBy>
            <GroupBy NAME="result" FROM_TABLE="AdaptersInfo">
                <AggregateColumn  NAME="OpCnt"
                    COLUMN_NAME ="OpCnt"
                    OPERATOR="SUM" />
            </GroupBy>    
            <JoinTables NAME="LoadAndResource">
                <Table NAME="result1"/>
                <Table NAME="result"/>
                <Table NAME="result2"/>
                <Column NAME="OpCnt" TABLE_NAME="result"/>
                <Column NAME="OpenConnections_raw" TABLE_NAME="result1"/>
                <Column NAME="NoofUsers" TABLE_NAME="result1"/>
                <Column NAME="NoofIPs" TABLE_NAME="result2"/>
            </JoinTables>
        </ExecutionDescriptor>
    </Metric>
