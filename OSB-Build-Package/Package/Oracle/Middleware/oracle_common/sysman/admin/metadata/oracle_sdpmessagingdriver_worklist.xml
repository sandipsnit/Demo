<!DOCTYPE TargetMetadata SYSTEM "../dtds/TargetMetadata.dtd" [
<!ENTITY j2ee_application_metrics SYSTEM "./j2ee_application_metrics.xmlp">
<!ENTITY j2ee_application_response SYSTEM "./j2ee_application_response.xmlp">
]>

<TargetMetadata META_VER="1.0" TYPE="oracle_sdpmessagingdriver_worklist" CATEGORY_PROPERTIES="VersionCategory;serverType"  RESOURCE_BUNDLE_PACKAGE="oracle.sysman.emai.rsc.sdp">
   <Display>
      <Label NLSID="oracle_sdpmessagingdriver_worklistNLSID">Worklist Driver</Label>
      <ShortName NLSID="oracle_sdpmessagingdriver_worklistShortName">Worklist Driver</ShortName>
      <Description NLSID="oracle_sdpmessagingdriver_worklistDescription">User Messaging Worklist Driver</Description>
   </Display>

   <AssocTarget ASSOCIATION_NAME="farm" ASSOC_TARGET_TYPE="*"/>
   <AssocTarget ASSOCIATION_NAME="managedBy" ASSOC_TARGET_TYPE="*"/>
   <AssocTarget ASSOCIATION_NAME="managedServer" ASSOC_TARGET_TYPE="*"/>

   &j2ee_application_response; 
   &j2ee_application_metrics;

   <Metric NAME="worklist_driver_message_counts_inst" TYPE="TABLE">
      <Display>
         <Label NLSID="worklist_driver_message_counts_inst">Message Counts</Label>
      </Display>
      <TableDescriptor>
         <ColumnDescriptor NAME="Received" TYPE="NUMBER">
            <Display>
               <Label NLSID="Received">Messages Received</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="ReceivingFailed" TYPE="NUMBER">
            <Display>
               <Label NLSID="ReceivingFailed">Messages Received Failed</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="Sent" TYPE="NUMBER">
            <Display>
               <Label NLSID="Sent">Messages Sent</Label>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="SendingFailed" TYPE="NUMBER">
            <Display>
               <Label NLSID="SendingFailed">Messages Sent Failed</Label>
            </Display>
         </ColumnDescriptor>
      </TableDescriptor>
      <QueryDescriptor FETCHLET_ID="JMX">
         <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
         <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
         <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
         <Property NAME="metric" SCOPE="GLOBAL">oracle_sdpmessaging:worklist_driver_message_counts_inst</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="appName.parameter" SCOPE="INSTANCE">appName</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">Received;ReceivingFailed;Sent;SendingFailed</Property>
         <Property NAME="admlMap" SCOPE="GLOBAL">
           <![CDATA[<argument type="java.util.Map"><value><item><key>appName.parameter</key><value type="java.lang.String">%appName.parameter%</value></item></value></argument>]]>
         </Property>
      </QueryDescriptor>
   </Metric>

   <Metric NAME="worklist_driver_performance_inst" TYPE="TABLE">
      <Display>
         <Label NLSID="worklist_driver_performance_inst">Driver Performance</Label>
      </Display>
      <TableDescriptor>
         <ColumnDescriptor NAME="ReceivingErrorRatio" TYPE="NUMBER">
            <Display>
               <Label NLSID="ReceivingErrorRatio">Error Ratio for Received Messages (%)</Label>
               <Unit NLSID="%">%</Unit>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="ReceivingThroughput" TYPE="NUMBER">
            <Display>
               <Label NLSID="ReceivingThroughput">Receiving Throughput (Messages/Second)</Label>
               <Unit NLSID="messages/sec">Messages/Second</Unit>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="ReceivingTime" TYPE="NUMBER">
            <Display>
               <Label NLSID="ReceivingTime">Receiving Latency (ms)</Label>
               <Unit NLSID="ms">ms</Unit>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="SendingErrorRatio" TYPE="NUMBER">
            <Display>
               <Label NLSID="SendingErrorRatio">Error Ratio for Sent Messages (%)</Label>
               <Unit NLSID="%">%</Unit>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="SendingThroughput" TYPE="NUMBER">
            <Display>
               <Label NLSID="SendingThroughput">Sending Throughput (Messages/Second)</Label>
               <Unit NLSID="messages/sec">Messages/Second</Unit>
            </Display>
         </ColumnDescriptor>
         <ColumnDescriptor NAME="SendingTime" TYPE="NUMBER">
            <Display>
               <Label NLSID="SendingTime">Sending Latency (ms)</Label>
               <Unit NLSID="ms">ms</Unit>
            </Display>
         </ColumnDescriptor>
      </TableDescriptor>
      <QueryDescriptor FETCHLET_ID="JMX">
         <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
         <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
         <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
         <Property NAME="metric" SCOPE="GLOBAL">oracle_sdpmessaging:worklist_driver_performance_inst</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="appName.parameter" SCOPE="INSTANCE">appName</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">ReceivingErrorRatio;ReceivingThroughput;ReceivingTime;SendingErrorRatio;SendingThroughput;SendingTime</Property>
         <Property NAME="admlMap" SCOPE="GLOBAL">
           <![CDATA[<argument type="java.util.Map"><value><item><key>appName.parameter</key><value type="java.lang.String">%appName.parameter%</value></item></value></argument>]]>
         </Property>
      </QueryDescriptor>
   </Metric>
   <InstanceProperties>

  <InstanceProperty NAME="version">
   <Display>
     <Label NLSID="j2ee_app_version_iprop">Version of J2EE Application</Label>
    </Display>
  </InstanceProperty>

  <InstanceProperty NAME="appName">
   <Display>
     <Label NLSID="j2ee_app_name_iprop">Name of the J2EE Application</Label>
    </Display>
  </InstanceProperty>

  <InstanceProperty NAME="appVersion" OPTIONAL="TRUE">
   <Display>
     <Label NLSID="j2ee_app_version_iprop">Version of J2EE Application</Label>
    </Display>
  </InstanceProperty>

  <InstanceProperty NAME="VersionCategory" IS_COMPUTED="TRUE">
   <Display>
    <Label NLSID="version_cat_iprop">Version Category</Label>
   </Display>
  </InstanceProperty>

    <InstanceProperty NAME="ObjectName" CREDENTIAL="FALSE" OPTIONAL="TRUE">
      <Display>
        <Label NLSID="object_name_prop">Object Name</Label>
      </Display>
    </InstanceProperty>

    <InstanceProperty NAME="ServerNames">
       <Display>
          <Label NLSID="serverNames">Server Names</Label>
       </Display>
    </InstanceProperty>

    <InstanceProperty NAME="serverType">
       <Display>
          <Label NLSID="server_type_iprop">Server Type</Label>
       </Display>
    </InstanceProperty>

<DynamicProperties NAME="ComputeVC" FORMAT="ROW" 
                   PROP_LIST="VersionCategory">
  <QueryDescriptor FETCHLET_ID="VersionRangeComputer">
    <Property NAME="Version" SCOPE="INSTANCE">version</Property>
    <Property NAME="DefaultRange" SCOPE="GLOBAL">11plus</Property>
    <Property NAME="11plus" SCOPE="GLOBAL">11.0.0;</Property>
  </QueryDescriptor>
</DynamicProperties>
   </InstanceProperties>
</TargetMetadata>

