<!DOCTYPE TargetMetadata SYSTEM "../dtds/TargetMetadata.dtd" 
>

<TargetMetadata CATEGORY_PROPERTIES="VersionCategory"
                META_VER="11.0"
                TYPE="oracle_toponode">

  <Display>
    <Label NLSID="oracle_toponode_name">Oracle MAS Topology Node</Label>
    <ShortName NLSID="oracle_toponode_name">Oracle MAS Topology Node</ShortName>
    <Description NLSID="oracle_toponode_desc">Oracle MAS Topology Node</Description>
  </Display>

  <InstanceProperties>
    <InstanceProperty NAME="MAS_OracleInstance">
      <Display>
        <Label NLSID="MAS_OracleInstanceLabel">MAS Oracle Instance Home</Label>
      </Display>
    </InstanceProperty>
    <InstanceProperty NAME="topologyNodeID">
      <Display>
        <Label NLSID="MAS_toplogyNodeIDLabel">Topology Node ID</Label>
      </Display>
    </InstanceProperty>
    <InstanceProperty CREDENTIAL="FALSE"
                      NAME="version"
                      OPTIONAL="FALSE">
      <Display>
        <Label NLSID="version_iprop">Version</Label>
      </Display>
    </InstanceProperty>
    <InstanceProperty IS_COMPUTED="TRUE"
                      NAME="VersionCategory">
      <Display>
        <Label NLSID="version_cat_iprop">Version Category</Label>
      </Display>
    </InstanceProperty>
    <DynamicProperties FORMAT="ROW"
                       NAME="ComputeVC"
                       PROP_LIST="VersionCategory">
      <QueryDescriptor FETCHLET_ID="VersionRangeComputer">
        <Property NAME="Version" SCOPE="INSTANCE">version</Property>
        <Property NAME="DefaultRange" SCOPE="GLOBAL">11plus</Property>
        <Property NAME="11plus" SCOPE="GLOBAL">11.0.0.0.0;</Property>
      </QueryDescriptor>
    </DynamicProperties>
  </InstanceProperties>
</TargetMetadata>
