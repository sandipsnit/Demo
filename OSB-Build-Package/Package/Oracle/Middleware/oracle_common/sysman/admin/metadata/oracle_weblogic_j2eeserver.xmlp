<!-- server -->
<Metric NAME="server" TYPE="TABLE">
  <ValidIf>
    <CategoryProp NAME="VersionCategory" CHOICES="11plus"/>
  </ValidIf>
  <Display>
    <Label NLSID="server">Server Metrics</Label>
  </Display>
  <TableDescriptor>
      <ColumnDescriptor NAME="startTime.value" TYPE="NUMBER" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_startTimeValue">Start Time</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="sockets.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_socketsActive">Active Sockets</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="socketsAccepted.completed" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE">
        <Display>
          <Label NLSID="server_socketsAcceptedCompleted">Socket Registrations Completed (since startup)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="listenPort.value" TYPE="NUMBER" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_listenPortValue">Listen Port</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="state.value" TYPE="NUMBER" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_stateValue">State</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="sslListenPort.value" TYPE="NUMBER" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_sslListenPortValue">SSL Listen Port</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="listenPort.enabled" TYPE="STRING" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_listenPortEnabled">Listen Port Enabled</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="sslListenPort.enabled" TYPE="STRING" TRANSIENT="TRUE" IS_KEY="FALSE">
        <Display>
          <Label NLSID="server_sslListenPortEnabled">SSL Listen Port Enabled</Label>
        </Display>
      </ColumnDescriptor>
 </TableDescriptor>
  <QueryDescriptor FETCHLET_ID="JMX">
         <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
         <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
         <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
         <Property NAME="metric" SCOPE="GLOBAL">weblogic_j2eeserver:server</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">startTime.value;sockets.active;socketsAccepted.completed;listenPort.value;state.value;sslListenPort.value;listenPort.enabled;sslListenPort.enabled</Property>
  </QueryDescriptor>
</Metric>

<!-- jvm -->
<Metric NAME="jvm" TYPE="TABLE">
  <ValidIf>
    <CategoryProp NAME="VersionCategory" CHOICES="11plus"/>
  </ValidIf>
  <Display>
    <Label NLSID="jvm">JVM Metrics</Label>
  </Display>
  <TableDescriptor>
       <ColumnDescriptor NAME="heapUsed.value" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_heapUsedValue">Heap Usage (MB)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="heapUsedPercentage.value" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_heapUsedPercentageValue">Heap Usage (%)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="jdkVendor.value" TYPE="STRING" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_jdkVendorValue">JDK Vendor</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="jdkVersion.value" TYPE="STRING" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_jdkVersionValue">JDK Version</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="cpuUsage.percentage" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_cpuUsagePercentage">CPU Usage (%)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="daemonThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_daemonThreadsActive">Active Daemon Threads</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="threads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_threadsActive">Active Threads</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="nurserySize.value" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="jvm_nurserySizeValue">Nursery Size (MB)</Label>
        </Display>
      </ColumnDescriptor>
 </TableDescriptor>
  <QueryDescriptor FETCHLET_ID="JMX">
         <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
         <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
         <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
         <Property NAME="metric" SCOPE="GLOBAL">weblogic_j2eeserver:jvm</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">heapUsed.value;heapUsedPercentage.value;jdkVendor.value;jdkVersion.value;cpuUsage.percentage;daemonThreads.active;threads.active;nurserySize.value</Property>
  </QueryDescriptor>
</Metric>

<!-- application -->
<Metric NAME="application" TYPE="TABLE"  USAGE_TYPE="COLLECT_UPLOAD">
  <ValidIf>
    <CategoryProp NAME="VersionCategory" CHOICES="11plus"/>
  </ValidIf>
  <Display>
    <Label NLSID="application">Application Metrics</Label>
  </Display>
  <TableDescriptor>
      <ColumnDescriptor NAME="appName" TYPE="STRING" IS_KEY="TRUE">
        <Display>
          <Label NLSID="application_name">Application Name</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="Status" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="application_status">Application - Status</Label>
        </Display>
      </ColumnDescriptor>
  </TableDescriptor>
  <QueryDescriptor FETCHLET_ID="JMX">
         <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
         <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
         <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
         <Property NAME="metric" SCOPE="GLOBAL">weblogic_j2eeserver:application</Property>
         <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
         <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
         <Property NAME="columnOrder" SCOPE="GLOBAL">appName;Status</Property>
         <Property NAME="appName.parameter" SCOPE="USER" OPTIONAL="TRUE">appName.parameter</Property>
         <Property NAME="useCache" SCOPE="GLOBAL">false</Property>
         <Property NAME="admlMap" SCOPE="GLOBAL">
           <![CDATA[<argument type="java.util.Map"><value><item><key>appName.parameter</key><value type="java.lang.String">%appName.parameter%</value></item></value></argument>]]>
         </Property>         
  </QueryDescriptor>
</Metric>

<!-- thread_pool -->
<Metric NAME="thread_pool" TYPE="TABLE" >
<!--
  <ValidIf>
    <CategoryProp NAME="VersionCategory" CHOICES="11plus"/>
  </ValidIf>
-->
  <Display>
    <Label NLSID="thread_pool">Thread Pool Metrics</Label>
  </Display>
  <TableDescriptor>
      <ColumnDescriptor NAME="requests.completed" TYPE="NUMBER" IS_KEY="FALSE" TRANSIENT="TRUE">
        <Display>
          <Label NLSID="thread_pool_requestsCompleted">Thread Pool - Requests Completed (since startup)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="requests.throughput" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_requestsThroughput">Thread Pool - Requests (per minute)</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="idleThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_idleThreads">Thread Pool - Idle Threads</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="totalThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_totalThreads">Thread Pool - Total Threads</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="useThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_useThreads">Thread Pool - Threads in Use</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="hoggedThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_hoggedThreads">Thread Pool - Hogging Threads</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="pendingUserRequests.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_pendingUserRequests">Thread Pool - Pending User Requests</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="pendingSystemRequests.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_pendingSystemRequests">Thread Pool - Pending System Requests</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="pendingRequests.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_pendingRequests">Thread Pool - Pending Requests</Label>
        </Display>
      </ColumnDescriptor>
      <ColumnDescriptor NAME="standbyThreads.active" TYPE="NUMBER" IS_KEY="FALSE">
        <Display>
          <Label NLSID="thread_pool_standbyThreads">Thread Pool - Standby Threads</Label>
        </Display>
      </ColumnDescriptor>
  </TableDescriptor>
  <QueryDescriptor FETCHLET_ID="JMX">
      <Property NAME="serviceURL" SCOPE="ASSOCTGT" ASSOCIATION_NAME="farm">serviceURL</Property>
      <Property NAME="UserName" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">UserName</Property>
      <Property NAME="password" SCOPE="ASSOCTGT" OPTIONAL="TRUE" ASSOCIATION_NAME="farm">password</Property>
      <Property NAME="metric" SCOPE="GLOBAL">weblogic_j2eeserver:thread_pool</Property>
      <Property NAME="ServerNames" SCOPE="INSTANCE">ServerNames</Property>
      <Property NAME="MetricService" SCOPE="GLOBAL">true</Property>
      <Property NAME="columnOrder" SCOPE="GLOBAL">requests.completed;requests.throughput;idleThreads.active;totalThreads.active;useThreads.active;hoggedThreads.active;pendingUserRequests.active;pendingSystemRequests.active;pendingRequests.active;standbyThreads.active</Property>
  </QueryDescriptor>
</Metric>

