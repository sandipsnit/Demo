	<!-- QMLoad -->
	<Metric NAME="QMLoad" TYPE="TABLE">
      <Display>
         <Label NLSID="QM_LOAD">Queue Manager Load</Label>
      </Display>
      <TableDescriptor>
	 <ColumnDescriptor NAME="DeltaBytesSent" TYPE="NUMBER" IS_KEY="FALSE">
            <Display>
               <Label NLSID="WMQ_LOAD_DBS">Bytes Sent</Label>
               <Description NLSID="WMQ_LOAD_DBS_DESC">Bytes sent since last collection</Description>
            </Display>
         </ColumnDescriptor>
	 <ColumnDescriptor NAME="DeltaBytesReceived" TYPE="NUMBER" IS_KEY="FALSE">
            <Display>
               <Label NLSID="WMQ_LOAD_DBR">Bytes Received</Label>
               <Description NLSID="WMQ_LOAD_DBR_DESC">Bytes received since last collection</Description>
            </Display>
         </ColumnDescriptor>
	 <ColumnDescriptor NAME="DeltaMessagesSentReceived" TYPE="NUMBER" IS_KEY="FALSE">
            <Display>
                <Label NLSID="WMQ_LOAD_DMSR">Messages Sent/Received</Label>
                <Description NLSID="WMQ_LOAD_DMSR_DESC">Messages sent and/or received since last collection</Description>
            </Display>
         </ColumnDescriptor>
		</TableDescriptor>
		<ExecutionDescriptor>
			<GetTable NAME="ChannelCurrentStatus" />
			<GetView NAME="VW_COMPRW" FROM_TABLE="ChannelCurrentStatus">
				 <Column NAME="DeltaBytesSent"/>
				 <Column NAME="DeltaBytesReceived"/>
				 <Column NAME="DeltaMessagesSentReceived"/>
			 </GetView>
			<GroupBy NAME="Name" FROM_TABLE="VW_COMPRW">
        		<AggregateColumn  NAME="DeltaBytesSent" COLUMN_NAME="DeltaBytesSent" OPERATOR="SUM"/>
				<AggregateColumn  NAME="DeltaBytesReceived" COLUMN_NAME="DeltaBytesReceived" OPERATOR="SUM"/>
				<AggregateColumn  NAME="DeltaMessagesSentReceived" COLUMN_NAME="DeltaMessagesSentReceived" OPERATOR="SUM"/>
			</GroupBy>
		</ExecutionDescriptor>
	</Metric>

