	<!-- SenderChannelQs -->
	<Metric NAME="SenderChannelQs" TYPE="TABLE">
      <Display>
         <Label NLSID="QM_SCQ">Sender Channel Queues</Label>
      </Display>
      <TableDescriptor>
			<ColumnDescriptor NAME="ChannelName" TYPE="STRING" IS_KEY="TRUE">
            <Display>
               <Label NLSID="QM_SCQ_CHANNAME">Channel Name</Label>
               <Description NLSID="QM_SCQ_CHANNAME_DESC">Channel name</Description>
            </Display>
         </ColumnDescriptor>
	 <ColumnDescriptor NAME="QName" TYPE="STRING" IS_KEY="TRUE">
            <Display>
               <Label NLSID="QM_SCQ_QNAME">Queue Name</Label>
               <Description NLSID="QM_SCQ_QNAME_DESC">Name of the remote queue</Description>
            </Display>
         </ColumnDescriptor>
			<ColumnDescriptor NAME="XmitQName" TYPE="STRING" IS_KEY="FALSE">
            <Display>
               <Label NLSID="QM_SCQ_XMITQNAME">Transmission Queue Name</Label>
               <Description NLSID="QM_SCQ_XMITQNAME_DESC">Transmission queue name</Description>
            </Display>
         </ColumnDescriptor>
		</TableDescriptor>
		<ExecutionDescriptor>
			<GetTable NAME="Channels"/>
			<GetView NAME="V_Channels" FROM_TABLE="Channels">
				<!-- <Column COLUMN_NAME="ChannelName"/>
				<Column COLUMN_NAME="XmitQName"/> -->
				<Filter COLUMN_NAME="Type" OPERATOR="EQ">Sender</Filter>
				<Filter COLUMN_NAME="XmitQName" OPERATOR="ISNOTNULL"/>
				<Filter COLUMN_NAME="ChannelName" OPERATOR="ISNOTNULL"/>
			</GetView>
			<GetTable NAME="Queues"/>
			<GetView NAME="V_Queues" FROM_TABLE="Queues">
				<!-- <Column COLUMN_NAME="QName"/>
				<Column COLUMN_NAME="XmitQName"/> -->
				<Filter COLUMN_NAME="Type" OPERATOR="EQ">Remote</Filter>
				<Filter COLUMN_NAME="XmitQName" OPERATOR="ISNOTNULL"/>
				<Filter COLUMN_NAME="QName" OPERATOR="ISNOTNULL"/>
				<In COLUMN_NAME="XmitQName" IN_TABLE_NAME="V_Channels" IN_COLUMN_NAME="XmitQName"/>
			</GetView>
			<JoinTables NAME="ret" OUTER="TRUE">
        		<Table NAME="V_Channels" />
        		<Table NAME="V_Queues" />
				<Where  FROM_TABLE="V_Channels" FROM_KEY="XmitQName" JOIN_TABLE="V_Queues" JOIN_KEY="XmitQName"/>
 		    </JoinTables>
 		    <GetView NAME="lastgetview" FROM_TABLE="ret">
