<targetAuth pluginName="AS">
        <secureTargetTypeList>
                <secureTargetType name="jboss_domain"/>
        </secureTargetTypeList>
        <urlPatternList>
              <urlPattern name="/em/faces/as/"/>
              <urlPattern name="/em/faces/ai/"/>
              <urlPattern name="/em/faces/emas-"/>
              <urlPattern name="/em/faces/emas_"/>
              <urlPattern name="/em/faces/patching-"/>
        </urlPatternList>
        <targetConnectionHandler className="oracle.sysman.emSDK.sec.targetauth.emas.EMASConnectionHandler"/>
        <targetLogin
                loginUrl="/em/faces/targetauth/emasLogin"
                userName="t_potatousername"
                password="t_potatopassword"
                saveCreds="t_potatosavecreds"
                credSetName="MASCredsPreferred"/>
        <homePage name="/em/faces/as/as/jbossDomainHome"/>
        <customParamList>
                <customParam name="potato_a" value="a_val"/>
                <customParam name="potato_b" value="b_val"/>
                <customParam name="potato_c" value="c_val"/>
                <customParam name="potato_d" value="d_val"/>
        </customParamList>
</targetAuth>

