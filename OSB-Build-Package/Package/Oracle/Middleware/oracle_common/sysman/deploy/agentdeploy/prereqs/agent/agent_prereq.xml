    <PREREQUISITE NAME="CheckingSudoPrivilege"
            EXTERNALNAME="Check whether remote user has root command privilege on remote nodes when atleast one execution is opted with root privilege"
            EXTERNALNAMEID="S_SUDO_CHECK@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes" SEVERITY="Error">
    <DESCRIPTION TEXT="This is a pre-requisite to check if the user has root command privilege on remote nodes when user opted any execution to be performed with root privilege"
            TEXTID="S_SUDO_CHECK_DESCRIPTION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    <RULESETREF NAME="CheckSudoPrivilege" RULE="CheckingSudoPrivilege" FILE="deploy/deploy_refhost.xml"
            RESULTS_FILE="prereq_results.xml"/>
