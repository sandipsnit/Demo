            TEXTID="S_SUDO_CHECK_ERROR@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    <RECOMMENDATION TEXT="Make sure that root command binary is installed on remote hosts or the user is configured to run root command or retry without opting any operation with root privilege"
            TEXTID="S_SUDO_CHECK_ACTION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    </PREREQUISITE>

