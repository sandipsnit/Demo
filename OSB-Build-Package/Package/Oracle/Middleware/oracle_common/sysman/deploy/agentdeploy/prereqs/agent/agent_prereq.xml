    <PREREQUISITE NAME="CheckSSH"
            EXTERNALNAME="Is SSH server running on the remote nodes?"
            EXTERNALNAMEID="S_CHECK_SSH_CONFIGURATION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes" SEVERITY="Error">
    <DESCRIPTION TEXT="This is a prerequisite to test whether SSH connection setup with the remote nodes exists from OMS; if not this will setup the SSH connection."
            TEXTID="S_CHECK_SSH_CONFIGURATION_DESCRIPTION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    <RULESETREF NAME="NetworkChecks" RULE="CheckSSH" FILE="deploy/deploy_refhost.xml"
            RESULTS_FILE="prereq_results.xml"/>
    <PROBLEM TEXT="Some of the nodes do not have SSH server running."
            TEXTID="S_CHECK_SSH_CONFIGURATION_ERROR@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    <RECOMMENDATION TEXT="Bring up the SSH server on the failed nodes." 
            TEXTID="S_CHECK_SSH_CONFIGURATION_ACTION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.PrereqsRuntimeRes"/>
    </PREREQUISITE>

    <PREREQUISITE NAME="SourceSoftwareCheck"
            EXTERNALNAME="Prerequisite checks for the Source Software Location."
            EXTERNALNAMEID="S_SOURCE_SOFT_CHECK@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.AgentPrereqRuntimeRes" SEVERITY="Error">
    <DESCRIPTION TEXT="This is a prerequisite to check whether the source software location provided exists on the remote nodes, and to check for the validity of the source software if present."
            TEXTID="S_SOURCE_SOFT_CHECK_DESCRIPTION@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.AgentPrereqRuntimeRes"/>
    <RULESETREF NAME="SourceSoftwareCheck" RULE="CheckSourceSoftware" FILE="deploy/deploy_refhost.xml"
            RESULTS_FILE="prereq_results.xml"/>
    <PROBLEM TEXT="Either the source software location is not visible from one or more hosts or the software present is not a valid agent software."
            TEXTID="S_SOURCE_SOFT_PROBLEM@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.AgentPrereqRuntimeRes"/>
    <RECOMMENDATION TEXT="Refer to the logs to find the exact cause of the failure and fix the problem." 
            TEXTID="S_SOURCE_SOFT_RECO@oracle.sysman.core.agentdeploy.deploy.prereqs.resources.AgentPrereqRuntimeRes"/>
    </PREREQUISITE>

