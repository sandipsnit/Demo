<?xml version="1.0" encoding="UTF-8"?>
<ada:AgentDeploy version="1"
                   xmlns:ada="http://www.oracle.com/sysman/deploy/agentdeploy"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.oracle.com/sysman/deploy/agentdeploy ThreadPoolConfig.xsd" >
    <ada:ThreadPool name="ADAThreadPool"
                     threadClass="oracle.sysman.core.agentdeploy.tp.AgentDeployWorkerImpl">
        <ada:ThreadPoolBoundries initVal="32" maxVal="128"/>
        <ada:ThreadPoolTimeouts waitBeforeDeny="10000" />
        <ada:ThreadPoolReturns returnNull="true" alternateClassName=""/>
    </ada:ThreadPool>
</ada:AgentDeploy>
