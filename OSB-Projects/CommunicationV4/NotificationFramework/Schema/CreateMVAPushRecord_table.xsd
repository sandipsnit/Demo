      <xs:sequence>
         <xs:element name="TbMvaPushDetails" type="TbMvaPushDetails" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TbMvaPushDetails">
      <xs:sequence>
         <xs:element name="osbTransactionId">
            <xs:simpleType>
