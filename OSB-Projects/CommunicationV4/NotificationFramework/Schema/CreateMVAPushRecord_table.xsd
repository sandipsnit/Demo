<?xml version = '1.0' encoding = 'UTF-8'?>
<xs:schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/top/CreateMVAPushRecord" xmlns="http://xmlns.oracle.com/pcbpel/adapter/db/top/CreateMVAPushRecord" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:element name="TbMvaPushDetailsCollection" type="TbMvaPushDetailsCollection"/>
