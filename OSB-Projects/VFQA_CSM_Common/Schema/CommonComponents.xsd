            <xsd:extension base="tns:PreferenceReferenceType">
                <xsd:sequence>
                    <xsd:element name="Explicit" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>An indication of whether explicit permission has been given for the preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ConfidenceRating" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The rating as to the confidence held that the preference is correct.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ExplicitThreshold" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The threshold value for the explicit permission/preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="PreferenceRoleType" type="cct:IDType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The role the preference should be applied to.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="InferredScore" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The score given to the preference as inferred by Vodafone.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="VolunteeredScore" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The score given to the preference as volunteered by the person providing the preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ValidityPeriod" type="tns:ValidityPeriodType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The period over which the preference is valid.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
