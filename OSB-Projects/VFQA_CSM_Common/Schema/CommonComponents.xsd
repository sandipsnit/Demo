	- Confidential --><xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://group.vodafone.com/schema/common/v1" version="1.1" xmlns:tns="http://group.vodafone.com/schema/common/v1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cct="urn:un:unece:uncefact:documentation:standard:CoreComponentType:2" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <!--======================================================================-->
    <!--===== Imports =====-->
    <!--======================================================================-->
    <xsd:import schemaLocation="CoreComponentType_2p0.xsd" namespace="urn:un:unece:uncefact:documentation:standard:CoreComponentType:2"/>
    <xsd:include schemaLocation="Meta.xsd"/>
    <xsd:include schemaLocation="CodeLists.xsd"/>
    <xsd:annotation>
        <xsd:documentation>Schema Definition for Common Components across CSM Services</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType name="ActorReferenceType">
        <xsd:annotation>
            <xsd:documentation>Generic Actor type object used to define roles.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PartyReferenceType">
        <xsd:annotation>
            <xsd:documentation>Party reference object used to refer a party.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="CharactersticsValueType">
        <xsd:sequence>
            <xsd:element name="Value" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The value of the characteristic</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Type" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A classification of the characteristic component. This identifies the sub-type of the instance of object or component.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Description" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A free text description of the characteristic.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CreationTime" type="cct:DateTimeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The date and time the characteristic was created.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ValidityPeriod" type="tns:ValidityPeriodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The from and to dates when the object is valid.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SpecificationType">
        <xsd:annotation>
            <xsd:documentation>A specification of the set of characteristics the define a particular object.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element name="CharacteristicsValue" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>The set of characteristics for the specification.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:complexContent>
                                <xsd:extension base="tns:CharactersticsValueType">
                                    <xsd:attribute name="characteristicName" type="xsd:string" use="optional">
                                        <xsd:annotation>
                                            <xsd:documentation>The name of the characteristic</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
                                        <xsd:annotation>
                                            <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:extension>
                            </xsd:complexContent>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="AreaOfInterestType">
        <xsd:sequence>
            <xsd:element name="AreaOfInterest" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="AreaOfInterest" type="cct:CodeType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <!--===== Sales Opportunity Reference-->
    <xsd:complexType name="SalesOpportunityReferenceType">
        <xsd:annotation>
            <xsd:documentation>An Opportunity is an opportunity to generate revenue from the account or lead. Opportunities are the pending deals that need to be tracked. Opportunities build the pipeline, which will contribute to the forecast.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Sales Account Reference-->
    <xsd:complexType name="SalesAccountReferenceType">
        <xsd:annotation>
            <xsd:documentation>Type definition for the SalesAccount. A sales account is a representation of a potential customer (prospect) around which sales activity is focused.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Sales Partner Account Reference-->
    <xsd:complexType name="SalesPartnerAccountReferenceType">
        <xsd:annotation>
            <xsd:documentation>A type definition for the sales partner account. Sales Partner Account is a special type of Partner Account that is used to manage the relationship with and activity of a Sales Partner</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Marketing Campaign ReferenceType-->
    <xsd:complexType name="MarketingCampaignReferenceType">
        <xsd:annotation>
            <xsd:documentation>A Marketing Campaign is a coordinated series of steps that can include the promotion of a Product Offering through different mediums (television, radio, print, online) using a variety of different types of advertisements. The campaign doesn't have to rely solely on advertising. It can also include demonstrations, word of mouth and other interactive techniques.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Market Segment Reference Type-->
    <xsd:complexType name="MarketSegmentReferenceType">
        <xsd:annotation>
            <xsd:documentation>A grouping of Parties, Geographic Areas, Sales Channels, and so forth. Market Segments are the target of Marketing Campaigns, Product Offerings, Product Promotions, Product Placements, and Product Programs from both internal and external (Competitors, and other Providers) perspective.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Sales Lead Reference Type-->
    <xsd:complexType name="SalesLeadReferenceType">
        <xsd:annotation>
            <xsd:documentation>A lead is the identification of an individual or organisation (Party) that has the interest and authority to purchase a product or service. This represents the first stage of a sales process. The identification of the lead is referred to as Lead Generation, a process conducted by either the marketing or sales organization. Thus, the Lead Entity in CIM is generated by the Lead Generation Interaction. This is a Party Interaction. The lead may have an organisation associated (
				an Enterprise Sales Lead) with the individual(s). For potential Enterprise Customers, The lead is considered to be a role of Organisation since the Lead individual will not become a customer per se. The individual represents the company (Legal Entity) with which Vodafone hope to do business.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Sales Order Reference-->
    <xsd:complexType name="SalesOrderReferenceType">
        <xsd:annotation>
            <xsd:documentation>A Sales Order that was created from a Quote.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Documents Reference-->
    <xsd:complexType name="DocumentsReferenceType">
        <xsd:annotation>
            <xsd:documentation>A document that could be attached to any entity.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="EventType">
        <xsd:annotation>
            <xsd:documentation>An Event happens at a Point in Time.
				An Event will
				always be the result of an Activity and often Triggers
				another
				Activity.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element name="EventImpact" type="cct:TextType" minOccurs="0"/>
                    <xsd:element name="InteractionEvent" type="cct:IndicatorType" minOccurs="0"/>
                    <xsd:element name="ValidityPeriod" type="tns:ValidityPeriodType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="IndividualRoleType">
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:element name="IndividualName" type="tns:IndividualNameType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Details associated to an individual name</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="BirthDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual was born.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="BloodType" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Blood Type of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DeathDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual died.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DeathIndicator" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Flag indicating whether the Individual has died</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Ethnicity" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Ethnicity of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Gender" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Gender of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Language" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Langauge of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="MaritalStatus" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Martial status of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Nationality" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Nationality of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DesignatedIndicator" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Designated indicator of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="TPSRegistration" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>TPS (Telephone preference service) registeration indicator for the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="FamilyGeneration" type="cct:CodeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="EN">An abbreviation or word that pertains to the generation in a family, such Sr, Jr.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ContactPointType">
        <xsd:sequence>
            <xsd:element name="ID" type="cct:IDType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The identifier of the business object or component. This should be used to identify instances of a business object or component. Where the component is being used to reference another business object then this is the "primary key" of the target object.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Preferred" type="cct:IndicatorType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">Indicates if the current contact point is the preferred one.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="Email" type="tns:EmailType">
                    <xsd:annotation>
                        <xsd:documentation>Email as contact point</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Telephone" type="tns:TelephoneType">
                    <xsd:annotation>
                        <xsd:documentation>Telephone as contact point</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Postal" type="tns:PostalAddressType">
                    <xsd:annotation>
                        <xsd:documentation>Postal as contact point</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="SMS" type="tns:SMSType">
                    <xsd:annotation>
                        <xsd:documentation>SMS as contact point</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Fax" type="tns:FaxType">
                    <xsd:annotation>
                        <xsd:documentation>SMS as contact point</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="TelephoneType">
        <xsd:sequence>
            <xsd:element name="PhoneType" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The phone type e.g. Landline, mobile</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubscriberNumber" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The subscriber number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LocalAreaCode" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The local area code for this telephone</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FullNumber" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The full number of this telephone</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="EmailType">
        <xsd:sequence>
            <xsd:element name="FullAddress" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The full email address e.g. peter.jackson@vodafone.com</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DomainName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The domain e.g. vodafone.com</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UserName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The username e.g. peter.jackson</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Verified" type="cct:IndicatorType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Whether the email address is a verified one or not</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="FaxType">
        <xsd:sequence>
            <xsd:element name="TypeCode" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The type code for the fax</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubscriberNumber" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The subscriber number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LocalAreaCode" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The local area code for the fax</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="IndividualNameType">
        <xsd:sequence>
            <xsd:element name="Title" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Is the title of the person. e.g. Mr., Dr. Ms.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FirstName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The primary chosen name. Also known as given name.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MiddleName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The person middle name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MiddleInitial" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The person middle initial name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FamilyName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The person family name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Prefix" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Prefix to the last name that is not considered part of the last name, such as German von or French dela, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Suffix" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Suffix to the last name, such as Senior, III, the Third etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Salutation" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Way the person is addressed e.g. Honorable etc</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FormattedName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Formatted name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AristocraticTitle" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Aristocratic Title e.g. Duke, Knight</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LegalName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Legal Name by which the person is addressed</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PreferredGivenName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name by which the person prefers to be addressed</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="SMSType">
        <xsd:sequence>
            <xsd:element name="TypeCode" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The type code of this SMS</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubscriberNumber" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The subscriber number e.g. 07765092245</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PostalAddressType">
        <xsd:sequence>
            <xsd:element name="Street" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The street, expressed as text, of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Town" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The town name, expressed as text, of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BuildingName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The building name, expressed as text, of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BuildingNumber" type="cct:NumericType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The building number expressed as text, for this address. e.g. 38, 55a</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Country" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The country for this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="County" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of the county in which the city falls, where applicable</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Occupant" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The occupant at this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PostCode" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A code specifying the post code of the address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PrimaryClassification" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The primary classification of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubLocality" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The sub locality of this address e.g district or borough</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubStreet" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The sub street of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubUnit" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The sub unit of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BuildingLetter" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The building letter of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TownSuffix" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The town suffic of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CountryCode" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The ISO 3166-2 country code of this address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Type" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The type of address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StreetSuffix" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A modifier to a street name often to denote a relative direction</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StreetType" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The kind of thoroughfare. Example alley, avenue, boulevard, crescent etc</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LevelNr" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The floor number within a building</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LevelType" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A description of the level within a building where numbers are not used</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PrivateStreetName" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The name of a street with a site. Example Street within a university</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Portal" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The portal name of the apartment or flat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PortalDoor" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The portal door number of the apartment or flat</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Characteristics" type="tns:CharacteristicsType" minOccurs="0"/>
            <xsd:element name="StreetNr" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">Number identifying a specific property on a public street. It may be combined with streetNrLast for ranged addresses</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StreetNrSuffix" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">The first street number suffix</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StreetNrLast" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">Last number in a range of street numbers allocated to a property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StreetNrLastSuffix" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">Last street number suffix for a ranged address</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="GeoLocation" type="tns:GeographicLocationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">The coordinates of the location</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="OrganisationRoleType">
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:element name="LocalTradingName" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The name of the organisation in its local area under which it trades. This may be its formal name or a colloquial version.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="GeoLocation" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>This is DEPRECATED and should not be used. The geographical location (latitude and longitude) of the organisation. The Geographical Location component should be used in preference to this field.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="AccountingPeriod" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The period through which the organisation measures its financial performance, e.g. Apr-Mar. Also known as the Financial Year of the organisation.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="CTPSRegisteration" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Indicates whether the organisation has registered with the Corporate Telephone Preference Service</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DissolutionDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>If the organisation no longer trades, this is the date on which it was closed down. Must comply with the ISO8601 standard.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="FoundationDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date on which the organisation was founded, but not necessarily incorporated.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="IncorporationDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date on which the organisation was incorporated, i.e. registered with the local Companies House or similar.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="NumberOfFamilyMembers" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The number of sub-organisations within this parent organisation. This may be interpreted as the number of departments in an organisation or the number of companies under an umbrella company.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="OrganisationStatusCode" type="cct:CodeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The status of the organisation. The Organisation/Status must be used in preference to this field.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="OrganisationStatusDate" type="tns:ValidityPeriodType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The start and end dates from which the status of the organisation applied.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="SICDescription" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The description of the Standard Industry Classification code.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="StandardIndustrialClassification" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The Standard Industry Classification Code for the organisation.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Industry" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The industry in which the organisation has primary operation.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="SubIndustry" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Part of segmentation process, used for marketing analysis.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="TaxIdentificationNumber" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Tax Identification Number (TIN). The registration number of the organisation with the federal/government tax office. In the U.K. this would be the HMRC.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Website" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>A reference to the organisation's main website. This field must comply with RFC3986.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ContactPoints" type="tns:ContactPointsType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The set of contact points for the organisation. A contact point being one of a postal address, email address, phone number.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ContactPeople" type="tns:ContactPeopleType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The set of people (individuals) within an organisation through which the organisation can be contacted.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
