    <xsd:complexType name="ScheduleType">
        <xsd:annotation>
            <xsd:documentation>A type that supports defining schedule jobs. It supports both one-time and recurring schedules. One-Time  - the task is executed at a specified datetime. Recurring - the task is executed repeatedly according to the specified frequency.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element name="FromDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date from which the schedule is effective.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ToDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date to which the schedule stop executing.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Recurrence" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="EN">The set of recurrent rules to execute the task.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="Frequency" type="cct:CodeType">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="EN">The frequency unit for the recurrence of the task.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element name="Interval" type="cct:NumericType" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="EN">The interval which determines how often the task will execute considering the specified frequency</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element name="Specific" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation xml:lang="EN">Specific settings of the schedule based on days, hours, minutes</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="Days" minOccurs="0">
                                                <xsd:annotation>
                                                    <xsd:documentation xml:lang="EN">Specific days when the task will be executed</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:choice>
                                                            <xsd:element name="NumericDay" type="cct:NumericType" minOccurs="0" maxOccurs="unbounded">
                                                                <xsd:annotation>
                                                                    <xsd:documentation xml:lang="EN">Day of the week when the schedule will run, depending on the frequency selected (week:1-7, month:1-31)</xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="NameDay" minOccurs="0" maxOccurs="unbounded">
                                                                <xsd:annotation>
                                                                    <xsd:documentation xml:lang="EN">Day(name) of the week when the schedule will run, applicable for week frequency</xsd:documentation>
                                                                </xsd:annotation>
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="Name" type="cct:CodeType" minOccurs="0">
                                                                            <xsd:annotation>
                                                                                <xsd:documentation xml:lang="EN">Day name, e.g.Monday, Tuesday, etc</xsd:documentation>
                                                                            </xsd:annotation>
                                                                        </xsd:element>
                                                                        <xsd:element name="Occurrence" type="cct:CodeType" minOccurs="0">
                                                                            <xsd:annotation>
                                                                                <xsd:documentation xml:lang="EN">List of possible occurrences, e.g. First, Last Used with Name and Frequency fields to specify schedule criteria like "Last Friday of the Week"</xsd:documentation>
                                                                            </xsd:annotation>
                                                                        </xsd:element>
                                                                    </xsd:sequence>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:choice>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="Times" minOccurs="0">
                                                <xsd:annotation>
                                                    <xsd:documentation xml:lang="EN">Specific times when the task should be executed. Used in cooperation with Days and Fequency component</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="Time" type="tns:ScheduleTimeType" minOccurs="0" maxOccurs="unbounded">
                                                            <xsd:annotation>
                                                                <xsd:documentation xml:lang="EN">Specific time when the task should be executed</xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:element>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="RunLimit" type="cct:NumericType" minOccurs="0">
                                    <xsd:annotation>
                                        <xsd:documentation>The number of times the task will be executed according to the schedule settings until it is considered completed.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Characteristics" type="tns:CharacteristicsType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="EN">The set of characteristics related to the schedule.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ScheduleTimeType">
        <xsd:sequence>
            <xsd:element name="Hour" type="cct:NumericType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="EN">Hour in a day when the task will be executed</xsd:documentation>
