                            <xsd:documentation>The date and time the note was created.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="LastModifiedDateTime" type="cct:DateTimeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Last Modified time of the notes</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PriceType">
