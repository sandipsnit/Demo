        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="MemberRoleType">
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:element name="Role" type="cct:IDType" minOccurs="0"/>
                    <xsd:element name="Influence" type="cct:IDType" minOccurs="0"/>
                    <xsd:element name="Opinion" type="cct:TextType" minOccurs="0"/>
                    <xsd:element name="ReportsTold" type="cct:IDType" minOccurs="0"/>
                    <xsd:element name="Urgency" type="cct:TextType" minOccurs="0"/>
                    <xsd:element name="BusinessGoals" type="cct:TextType" minOccurs="0"/>
                    <xsd:element name="DecisionCriteria" type="cct:TextType" minOccurs="0"/>
                    <xsd:element name="PersonalGoals" type="cct:TextType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ContactPeopleType">
        <xsd:sequence>
            <xsd:element name="ContactPerson" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tns:ContactPersonType">
                            <xsd:sequence>
                                <xsd:element name="ContactPoints" minOccurs="0">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="ContactPoint" type="tns:ContactPointType" minOccurs="0" maxOccurs="unbounded"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ContactPointsType">
        <xsd:sequence>
            <xsd:element name="ContactPoint" type="tns:ContactPointType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Contact Point. The choice being Postal, Telephone, Fax and SMS</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ContactPersonType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element name="JobFunctions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The set of job functions that the contact person is employed for.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="JobFunction" type="tns:JobFunctionType" maxOccurs="unbounded">
                                    <xsd:annotation>
                                        <xsd:documentation>A specific job function of the contact person</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="JobTitle" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Job Title e.g Manager, CEO, Consultant etc</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Seniority" type="cct:IDType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The seniority of the contact person.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ContactPreferences" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The set of contact preferences for the contact person. May include time of day, contact point, form of address, etc.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="ContactPreference" type="tns:ContactPersonContactPreferenceType" maxOccurs="unbounded">
                                    <xsd:annotation>
                                        <xsd:documentation>A specific contact preference for a contact person. May include time of day, contact point, form of address, etc.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="IndividualName" type="tns:IndividualNameType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The details of an individuals name.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="BirthDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual was born.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="BloodType" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Blood Type of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DeathDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual died.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DeathIndicator" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Flag indicating whether the Individual has died</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Ethnicity" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Ethnicity of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Gender" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Gender of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Language" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Langauge of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="MaritalStatus" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Martial status of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Nationality" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Nationality of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="DesignatedIndicator" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Designated indicator of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="TPSRegistration" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>TPS (Telephone preference service) registeration indicator for the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="FamilyGeneration" type="cct:CodeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="EN">An abbreviation or word that pertains to the generation in a family, such Sr, Jr.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="JobFunctionType">
        <xsd:sequence>
            <xsd:element name="Name" type="cct:TextType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of the job function</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Type" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Type of the job function</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="ContactPersonContactPreferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:PreferenceType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== ProductOffering Reference-->
    <xsd:complexType name="ProductOfferingReferenceType">
        <xsd:annotation>
            <xsd:documentation>The presentation of one or more Product Specifications to the marketplace for sale, rental, or lease for a Product Offering Price.
				A Product Offering may target one or more Market Segments, be included in one or more Product Catalogue, presented in support of one or more Product Strategies,
				and made available in one or more Places. Product Offering may represent a simple offering of a single Product Specification or could represent a bundling
				of one or more other Product
				Offering.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== SolutionOffering Reference-->
    <xsd:complexType name="SolutionOfferingReferenceType">
        <xsd:annotation>
            <xsd:documentation>Especially for Enterprise Customers, the product offering that forms the basis of the Quote will be customised for a specific customer.
				This Customised Offering is not the same as a Product although the final 'accepted' customised offering will be the specification for the product build.
				Customised Offerings may go through multiple versions before being accepted.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Condition Reference-->
    <xsd:complexType name="ConditionReferenceType">
        <xsd:annotation>
            <xsd:documentation>Condition</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== ProductCatalogue Reference-->
    <xsd:complexType name="ProductCatalogueReferenceType">
        <xsd:annotation>
            <xsd:documentation>A list of ProductOfferings for sale, with prices and illustrations, for example in book form or on the web.
				ProductCatalogs can be used by Customers during a self-care ordering process and may be used across one or more DistributionChannels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ApprovalReferenceType">
        <xsd:annotation>
            <xsd:documentation>A group of Authorizations required from the Business Participants involved in the entity being approved.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ApprovalType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:ApprovalReferenceType">
                <xsd:sequence>
                    <xsd:element name="CreationDateTime" type="cct:DateTimeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was created.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="RequestedDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was requested.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="CompletedDate" type="cct:DateType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was completed.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Outcome" type="cct:TextType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>A free text description of the outcome of the approval.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ChannelPreferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element name="Consent" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>An indication of whether the channel preference was given as consent by the consenter.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Channel" type="tns:ChannelReferenceType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The specific channel the preference applies to.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="Explicit" type="cct:IndicatorType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>An indication of whether explicit permission has been given for the preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ExplicitThreshold" type="cct:NumericType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The threshold value for the explicit permission/preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ValidityPeriod" type="tns:ValidityPeriodType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>The period over which the preference is valid.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ChannelReferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PermissionType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
