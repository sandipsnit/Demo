    <xsd:complexType name="StatusHistoryType">
        <xsd:annotation>
            <xsd:documentation>A history of the lifecycle state transistions of a VBO. Each transition records the date and time and should record the target lifecycle status.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="StateTransition" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ID" type="cct:IDType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>An identifier or name for the state transition. Examples include: "Activate", "Cancel", "Complete", etc.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="FromStatusCode" type="cct:CodeType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The source lifecycle state of the transition, i.e. the state from which the transition is moving, for example: from "Active" to "Cancelled".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ToStatusCode" type="cct:CodeType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The target lifecycle state of the transition, i.e. the state to which the transition is moving, for example: from "Active" to "Cancelled".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="EffectiveDateTime" type="cct:DateTimeType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The date and time of the transistion, i.e. the timestamp of when the target state was entered.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ReasonCode" type="cct:CodeType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The code describing why the transition occured.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ReasonText" type="cct:TextType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The reason code describing why the transition occured</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SubReasonCode" type="cct:CodeType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The Sub reason code describing why the transition occured.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SubReasonText" type="cct:TextType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The sub-reason code describing why the transition occured</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="Description" type="cct:TextType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>A description of the state transition.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ChangedByUserID" type="cct:IDType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>A reference to the User [Account] who initiated the lifecycle state transition.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="GeographicLocationType">
        <xsd:sequence>
            <xsd:element name="AltitudeMeasure" type="cct:MeasureType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The measure of the altitude that reflects the vertical elevation of an object above a surface for this geographical coordinate (Reference ISO 6709</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LatitudeMeasure" type="cct:MeasureType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The measure of the latitude as an angular distance north or south from the Equator meridian to the meridian of a specific place for this geographical coordinate. (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LongitudeMeasure" type="cct:MeasureType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The measure of the longitude as an angular distance east or west from the Greenwich meridian to the meridian of a specific place (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LatitudeDirectionCode" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The indication of whether the latitude compass direction from the Equator meridian to the meridian of a specific place is North (+) or South (-). (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LongitudeDirectionCode" type="cct:CodeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The indication of whether the longitude as a compass direction from the Greenwich meridian to the meridian of a specific place is East (-) or West (+) for this geographical coordinate. (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SystemID" type="cct:IDType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A unique identifier of the system used for measuring a geographical coordinate other than Global Positioning System</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="CredentialsGroupType">
